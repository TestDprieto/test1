    <validationRules>
        <fullName>vlocity_cmt__Active_Web_TrackingConfiguration</fullName>
        <active>true</active>
        <description>Only active Web Tracking Configuration records are allowed to be selected with Tracking Group.</description>
        <errorConditionFormula>AND(vlocity_cmt__VlocityWebTrackingConfigurationId__c &lt;&gt; null,vlocity_cmt__VlocityWebTrackingConfigurationId__r.vlocity_cmt__IsActive__c == false)</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__VlocityWebTrackingConfigurationId__c</errorDisplayField>
        <errorMessage>Only active Web Tracking Configuration records are allowed to be selected with Tracking Group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__Effective_Start_Date_and_End_Date</fullName>
        <active>true</active>
        <description>Effective End Date can not be in the past relative to Effective Start Date.</description>
        <errorConditionFormula>AND(ISBLANK(vlocity_cmt__EffectiveStartDate__c) = false, ISBLANK(vlocity_cmt__EffectiveEndDate__c) = false, vlocity_cmt__EffectiveStartDate__c - vlocity_cmt__EffectiveEndDate__c &gt; 0)</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__EffectiveEndDate__c</errorDisplayField>
        <errorMessage>End Date can not be older than Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__Max_Age_Greater_Than_Zero</fullName>
        <active>true</active>
        <description>Max Age In Days entry should be greater than zero.</description>
        <errorConditionFormula>vlocity_cmt__MaxAgeInDays__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__MaxAgeInDays__c</errorDisplayField>
        <errorMessage>Max Age In Days entry should be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__Oldest_EffectiveEndDate</fullName>
        <active>true</active>
        <description>Restricting end date to 2020/01/01.</description>
        <errorConditionFormula>DATE(2020, 1, 1) &gt;  vlocity_cmt__EffectiveEndDate__c</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__EffectiveEndDate__c</errorDisplayField>
        <errorMessage>End date can not be older that Jan 2020.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__Oldest_EffectiveStartDate</fullName>
        <active>true</active>
        <description>Restricting start date to 2020/01/01.</description>
        <errorConditionFormula>DATE(2020, 1, 1) &gt;  vlocity_cmt__EffectiveStartDate__c</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__EffectiveStartDate__c</errorDisplayField>
        <errorMessage>Start date can not be older that Jan 2020.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__TrackingConfig_With_Internal_Type</fullName>
        <active>true</active>
        <description>When Type is Internal, Web Tracking Configuration look up must be empty.</description>
        <errorConditionFormula>AND(ISPICKVAL(vlocity_cmt__Type__c, &apos;Internal&apos;), vlocity_cmt__VlocityWebTrackingConfigurationId__c &lt;&gt; null)</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__VlocityWebTrackingConfigurationId__c</errorDisplayField>
        <errorMessage>Vlocity Web Tracking Configuration with Tracking Group type Internal is invalid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__Type_Validation</fullName>
        <active>true</active>
        <description>When Type is Web, Web Tracking Configuration look up must be populated.</description>
        <errorConditionFormula>AND(ISPICKVAL(vlocity_cmt__Type__c, &apos;Web&apos;), vlocity_cmt__VlocityWebTrackingConfigurationId__c == null)</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__VlocityWebTrackingConfigurationId__c</errorDisplayField>
        <errorMessage>Please select a Web Tracking Configuration.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
