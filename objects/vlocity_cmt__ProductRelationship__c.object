        <fullName>vlocity_cmt__Default_quntity_cannot_be_zero</fullName>
        <active>true</active>
        <description>Default quantity should not be zero.</description>
        <errorConditionFormula>vlocity_cmt__DefaultQuantity__c = 0</errorConditionFormula>
        <errorMessage>Default quantity should not be zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__Min_Max_should_build_a_valid_range</fullName>
        <active>true</active>
        <description>Min &amp; Max should build a valid range</description>
        <errorConditionFormula>NOT ( AND ( (vlocity_cmt__MinQuantity__c  &lt;= vlocity_cmt__MaxQuantity__c),(  vlocity_cmt__DefaultQuantity__c &lt;= vlocity_cmt__MaxQuantity__c),( vlocity_cmt__DefaultQuantity__c &gt;= vlocity_cmt__MinQuantity__c)) )</errorConditionFormula>
        <errorMessage>Min Quantity should be less than or equal to Max Quantity And Default Quantity should be in between Min &amp; Max Quantity</errorMessage>
    </validationRules>
