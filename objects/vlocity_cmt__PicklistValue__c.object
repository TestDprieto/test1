        <formula>IF(  ISPICKVAL(vlocity_cmt__PicklistId__r.vlocity_cmt__DataType__c, &apos;Date Time&apos;) ,  TEXT(vlocity_cmt__DateTimeValue__c) ,
    IF( ISPICKVAL(vlocity_cmt__PicklistId__r.vlocity_cmt__DataType__c, &apos;Date&apos;) ,  TEXT(vlocity_cmt__DateValue__c) ,
        IF( ISPICKVAL(vlocity_cmt__PicklistId__r.vlocity_cmt__DataType__c, &apos;Text&apos;),  vlocity_cmt__TextValue__c ,
            IF( ISPICKVAL(vlocity_cmt__PicklistId__r.vlocity_cmt__DataType__c, &apos;Number&apos;) ,  (TEXT(vlocity_cmt__NumberValue__c) &amp; vlocity_cmt__UnitOfMeasureName__c) ,
                IF( ISPICKVAL(vlocity_cmt__PicklistId__r.vlocity_cmt__DataType__c, &apos;Boolean&apos;)  ,  IF( vlocity_cmt__BooleanValue__c = true  ,  &apos;true&apos; , &apos;false&apos; ), null )))))</formula>
