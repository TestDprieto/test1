    <validationRules>
        <fullName>vlocity_cmt__MaximumNumberOfRetriesValidation</fullName>
        <active>true</active>
        <description>MaximumNumberOfRetries field validation</description>
        <errorConditionFormula>NOT(vlocity_cmt__MaximumNumberOfRetries__c &gt;= 0)</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__MaximumNumberOfRetries__c</errorDisplayField>
        <errorMessage>Maximum Number Of Retries cannot be a negative number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__RetryIntervalsValidation</fullName>
        <active>true</active>
        <description>Retry Intervals field validation</description>
        <errorConditionFormula>AND(NOT(ISBLANK(vlocity_cmt__RetryIntervals__c)), NOT(REGEX(vlocity_cmt__RetryIntervals__c, &quot;(\\d+)(,\\s*\\d+)*&quot;)))</errorConditionFormula>
        <errorDisplayField>vlocity_cmt__RetryIntervals__c</errorDisplayField>
        <errorMessage>Retry Intervals must be whole numbers separated by commas.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_cmt__TimeBetweenRetriesValidation</fullName>
