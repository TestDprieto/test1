            "%vlocity_namespace%__Sass__c": "/*** VARIABLES ***/\n@import \"variables\";\n\n/*** MIXINS ***/\n@import \"mixin\";\n\n/*** STYLES ***/\nbody {\n    background: $light-gray;\n    padding: 0;\n    margin: 0;\n}\n\n.vlocity {\n    ul li, ol li {\n        margin-left:0px;\n    }\n\n    a {\n        &:hover, &:active, &:focus, &:visited {\n            text-decoration: none;\n        }\n    }\n\n    .cards-container {\n        @include clearfix;\n        overflow: hidden;\n        width: auto;\n        padding:10px;\n        position:relative;\n        background-color: $light-gray;\n        form {\n            padding:10px;\n        }\n    }\n\n    section.console-card {\n        float: left;\n        width: 31.5165%;/*fallback */\n        width: calc(100% / 3 - 20px);\n        margin: 10px;\n        min-height: 360px;\n        max-height: 360px;\n        overflow-y: auto;\n        background-color: $white;\n        box-shadow: 0 0 5px 0 rgba(0,0,0,.15);\n        box-sizing:border-box;\n        \n        &.selected {\n            outline:2px solid #05A6E0;\n        }\n        &.hotkey-highlight {\n            outline:2px solid #67CCEC;\n            &.selected {\n                outline:2px solid #05A6E0;\n            }\n        }\n        \n        .card-top {\n            padding: 15px 20px 7px;\n            border-bottom: 2px solid $light-gray;\n            @include clearfix;\n\n            h2 {\n                margin: 0 0 4px 0;\n                text-transform: uppercase;\n                font: $font-light;\n                font-size: 28px;\n                line-height: 28px;\n                height: 28px;\n                overflow: hidden;\n                text-overflow: ellipsis;\n                white-space: nowrap;\n                cursor: default;\n                display: inline-block;\n                max-width: 100%;\n            }\n\n            span {\n                letter-spacing: 0.02em;\n\n                &.account-number {\n                    float: left;\n                    font-size: 14px;\n                    font-weight: 600;\n                }\n\n                &.expires {\n                    float: right;\n                    font-size: 12px;\n                    position: relative;\n\n                    i.icon {\n                        color: $warning;\n                        font-size: 22px;\n                        position: absolute;\n                        top: -3px;\n                        left: -32px;\n\n                        &+ .tooltip .tooltip-inner {\n                            text-transform: none;\n                        }\n                    }\n                }\n            }\n        }\n    }\n\n    section.console-flyout {\n        float: left;\n        position:relative;\n        cursor:default;\n        width:98%; /*fallback*/\n        min-height: 300px;\n        width:calc(100% - 20px);\n        margin: 10px;\n        background-color: $white;\n        box-shadow: 0 0 5px 0 rgba(0,0,0,.15);\n        box-sizing:border-box;\n        \n        &.flyout {\n            transition:all 0.3s ease-in-out;\n            .animate-leave {\n              opacity: 1;\n              top: 0;\n              border:10px red;\n              height: 30px;\n            }\n            .card-info {\n                padding-top:15px;\n            }\n            .flyout-arrow {\n                display: inline-block;\n                position: absolute;\n                z-index: 10;\n                border-bottom: 18px solid #ffffff;\n                border-left: 18px solid transparent;\n                border-right: 18px solid transparent;\n                height: 0;\n                width: 0;\n                pointer-events: none;\n                bottom: 100%;\n                top:-17px;\n            }\n            .flyout-close {\n                display:inline-block;\n                position:absolute;\n                right:5px;\n                top:5px;\n                cursor:pointer;\n                i {\n                    font-size:30px;\n                }\n            }\n            \n            .card-info {\n                @include clearfix;\n                padding: 5px 20px;\n\n                ul {\n                    list-style: none;\n                    padding: 0;\n                    margin: 0;\n\n                    &.details {\n                        float: left;\n                        padding: 10px 5px 0px 0px;\n                        width: 48%;/* fallback */\n                        width: calc(100% - 150px); //.actions width\n                        cursor:pointer;\n                        word-wrap: break-word;\n                        li {\n                            margin-bottom: 16px;\n\n                            &:last-of-type {\n                                margin: 0;\n                            }\n                        }\n\n                        span {\n                            display: block;\n\n                            &.detail-label {\n                                font-weight: 600;\n                                font-size: 12px;\n                                text-transform: uppercase;\n                            }\n\n                            &.value {\n                                text-transform: none;\n                                color: $text-gray;\n                                font-size: 14px;\n                                font-weight: 300;\n                            }\n                        }\n                    }\n\n                    &.actions {\n                        float: right;\n                        width: 150px;\n                        word-wrap: break-word;\n\n                        a {\n                            display: table;\n                            width: 100%;\n                            height: 53px;\n                            border-bottom: 2px solid $light-gray;\n\n                            img {\n                                width: 29px;\n                                height: 29px;\n                                margin: 5px 9px;\n                                display: table-cell;\n                                vertical-align: middle;\n                            }\n\n                            i.icon {\n                                display: table-cell;\n                                vertical-align: middle;\n                                font-size: 25px;\n                                line-height: 34px;\n                                color: $light-blue;\n                                width: 40px;\n                                \n                                &:before {\n                                    font-weight: 100;\n                                    line-height: 34px;\n                                }\n                            }\n\n                            span {\n                                font-size: 12px;\n                                color: $text-gray;\n                                line-height: 1;\n                                display: table-cell;\n                                vertical-align: middle;\n                            }\n\n                            &:hover {\n                                text-decoration: none;\n\n                                i.icon {\n                                    color: $dark-blue;\n                                }\n\n                                span {\n                                    color: $light-blue;\n                                }\n                            }\n                        }\n\n                        li:last-of-type {\n                            a {\n                                border: none;\n                            }\n                        }\n                    }\n                }\n            }\n        }\n    }\n    /*End of Flyout specific CSS*/\n\n    @media only screen and (min-width : 1575px) {\n        .cards-container {\n            padding:10px;\n            section.console-card {\n                width: 23.4%; /* fallback */\n                width: calc(100% / 4 - 20px);\n                margin: 10px 10px 10px;\n            }\n            section.console-flyout {\n                &.flyout {\n                    margin: 10px 10px 10px;\n                    width: calc(100% - 20px);\n                    .flyout-arrow {\n                        border-width: 12px;\n                        margin-left: -12px;\n                        top:-12px;\n                    }\n                }\n            }\n        }\n    }\n\n    @media only screen and (max-width : 1050px) {\n        .cards-container {\n            padding:7px;\n            section.console-card {\n                width: 48.4%; /* fallback */\n                width: calc(100% / 2 - 14px);\n                margin: 7px 7px 7px;\n            }\n            section.console-flyout {\n                &.flyout {\n                    margin: 7px 7px 7px;\n                    width: calc(100% - 14px);\n                    .flyout-arrow {\n                        border-width: 12px;\n                        margin-left: -12px;\n                        top:-12px;\n                    }\n                }\n            }\n        }\n    }\n\n    @media only screen and (max-width : 725px) {\n        .cards-container {\n            padding:7px;\n            section.console-card {\n                width: 98%; /* fallback */\n                width: calc(100% - 14px);\n                margin: 7px 7px 7px;\n            }\n        }\n    }\n}",
