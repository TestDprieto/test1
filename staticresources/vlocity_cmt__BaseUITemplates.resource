            "%vlocity_namespace%__HTML__c": "<div class=\"via-slds\">\n    <div class=\"slds-grid slds-p-around--small slds-wrap via-slds-story-cards--header slds-theme--shade\">\n        <div class=\"slds-p-right--x-small via-slds-story-cards--header-title\">\n          <div class=\"slds-text-heading--medium\">{{$root.search.title ? $root.search.title + 's' : 'Customer Story'}}</div>\n        </div>\n        <div class=\"slds-col--bump-left slds-p-left--x-small via-slds-story-cards--header-title\" ng-if=\"cards.length > 0\"\n            ng-init=\"uniqueCards = (cards | uniqueValues:'title':{title: 'All'} | orderBy:'title'); isOpen = false\">\n            <div class=\"slds-dropdown-trigger slds-dropdown-trigger--click\" ng-class=\"{'slds-is-open': isOpen}\" aria-expanded=\"true\">\n              <button class=\"slds-button slds-button--icon-border-filled slds-button--icon-x-small\" aria-haspopup=\"true\" ng-click=\"isOpen = !isOpen\">\n                <slds-button-svg-icon size=\"'small'\" sprite=\"'utility'\" icon=\"'down'\"></slds-button-svg-icon>\n                <span class=\"slds-assistive-text\">Show More</span>\n              </button>\n              <div class=\"slds-dropdown slds-dropdown--right slds-dropdown--actions\">\n                <ul class=\"dropdown__list\" role=\"menu\">\n                  <li class=\"slds-dropdown__item\" ng-class=\"{'slds-is-selected': item.title === $root.search.title || (!$root.search.title && item.title === 'All')}\" ng-repeat=\"item in ::uniqueCards\">\n                    <a role=\"menuitem\"\n                        href=\"javascript:void(0)\"\n                        ng-click=\"$parent.isOpen = !$parent.isOpen; $root.search.title = (item.title === 'All' ? undefined : item.title)\">\n                      <p class=\"slds-truncate\">\n                        <slds-svg-icon size=\"'x-small'\" extra-classes=\"'slds-icon--selected slds-m-right--x-small'\" sprite=\"'utility'\" icon=\"'check'\"></slds-svg-icon>\n                