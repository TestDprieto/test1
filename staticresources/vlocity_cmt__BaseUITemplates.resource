            "%vlocity_namespace%__CSS__c": ".vlocity .account-profile .spacer {\n  background-color: white;\n  height: 40px;\n  box-sizing: border-box;\n  text-transform: uppercase;\n  text-align: left;\n  font-size: 12px;\n  color: black;\n  box-sizing: border-box;\n  padding: 9px;\n  width: inherit;\n  border-top: 2px solid #e6e6ec;\n  border-bottom: 2px solid #e6e6ec;\n  cursor: pointer; }\n  .vlocity .account-profile .spacer:hover {\n    color: #05a6df; }\n  .vlocity .account-profile .spacer i::before {\n    color: #1397db;\n    font-size: 14px; }\n\n.vlocity .account-profile .acct-profile {\n  padding: 0 0 0 5px;\n  background-color: white;\n  overflow: auto; }\n  .vlocity .account-profile .acct-profile ul {\n    padding: 5px 0 0 0;\n    clear: both; }\n    .vlocity .account-profile .acct-profile ul h3 {\n      text-transform: uppercase;\n      font-size: 12px;\n      font-weight: bold; }\n    .vlocity .account-profile .acct-profile ul li {\n      list-style: none;\n      display: inline;\n      float: left;\n      overflow: auto;\n      background: #9191a0;\n      /*#9191a0;*/\n      color: #fefefe;\n      /*#fefefe;*/\n      font-size: 12px;\n      padding: 7px 15px;\n      margin: 5px 10px 5px 5px; }",
