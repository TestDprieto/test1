            "%vlocity_namespace%__CSS__c": ".vlocity .console-account-info table {\n  padding-right: 4px; }\n  .vlocity .console-account-info table td strong {\n    font-size: 12px;\n    text-transform: uppercase; }\n\n.vlocity .console-account-info .spacer {\n  background-color: white;\n  height: 40px;\n  box-sizing: border-box;\n  text-transform: uppercase;\n  text-align: left;\n  font-size: 12px;\n  color: black;\n  box-sizing: border-box;\n  padding: 9px;\n  width: inherit;\n  border-top: 2px solid #e6e6ec;\n  border-bottom: 2px solid #e6e6ec;\n  cursor: pointer; }\n\n.vlocity .console-account-info .spacer:hover {\n  color: #05a6df; }\n\n.vlocity .console-account-info .spacer i::before {\n  color: #1397db;\n  font-size: 14px; }\n\n.vlocity .console-account-info .acct-info {\n  background-color: white; }\n\n.vlocity .console-account-info .acct-info td {\n  padding: 9px;\n  font-size: 12px;\n  vertical-align: top; }\n\n.vlocity .console-account-info #mailing-address {\n  text-transform: capitalize; }\n\n.vlocity .console-account-info .acct-links {\n  text-align: center;\n  font-size: 10px;\n  padding: 10px;\n  overflow: auto; }\n  .vlocity .console-account-info .acct-links ul {\n    padding: 0;\n    display: inline-block; }\n    .vlocity .console-account-info .acct-links ul li {\n      list-style-type: none;\n      float: left;\n      margin: 10px 0px 0px 0px; }\n      .vlocity .console-account-info .acct-links ul li a {\n        color: #1397db;\n        font-size: 10px;\n        padding: 5px;\n        cursor: pointer;\n        text-transform: uppercase; }\n      .vlocity .console-account-info .acct-links ul li .separator {\n        line-height: 15px;\n        padding: 0px 2px; }",
