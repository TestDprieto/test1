            "%vlocity_namespace%__Sass__c": "$light-blue: #05a6df;\n.vlocity {\n    .account-story {\n    \n    .spacer {\n        background-color: rgb(255,255,255);\n        height: 40px;\n        box-sizing: border-box;\n        text-transform: uppercase;\n        text-align: left;\n        font-size: 12px;\n        color: rgb(0,0,0);\n        box-sizing: border-box;\n        padding: 9px;\n        width: inherit;\n        border-top: 2px solid rgba(230,230,236,1);     \n        border-bottom: 2px solid rgba(230,230,236,1); \n        cursor: pointer;\n    }\n    .spacer:hover {\n        color: #05a6df;\n    }\n    .spacer i::before {\n        color: rgb(19, 151, 219);\n        font-size: 14px;\n    }\n    \n    #story-time {\n        background-color: white;\n    }\n    .story-type {\n        font-size: 12px;\n        margin-left: 5px;\n    }\n    .story {\n        font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;\n        font-size: 11px;\n        \n        /*position: relative;\n        height: 65px;*/\n        position: static;\n        height: auto;\n        \n        width: 100%;\n        padding: 10px 0;\n        cursor: pointer;\n        \n        &:hover {\n            background-color: #f2f2f2;\n        }\n        \n        &.open-oppty {\n            background-color: rgba(0, 164, 83, 0.2);\n        }\n        \n        &.open-case {\n            background-color: rgba(235, 33, 46, 0.2);\n        }\n    }\n    #story-time .story .line {\n        height: 40px;\n        width: 1px;\n        background-color: #ccc;\n        color: rgb(0,0,0);\n    }\n    #story-time .story:last-child .line {\n        height: 0;\n        width: 0px;\n    }\n    .icon-segment {\n        float:left;\n        position: relative; /* added */\n        .story-icon {\n            font-size: 24px;\n            line-height: 24px;\n            color: $light-blue;\n            width: 30px;\n        \n            &:before {\n                font-weight: 100;\n                line-height: 24px;\n            }\n        }\n    }\n    .icon-segment img {\n        position: absolute;\n        left: 9px;\n        top: 11px;\n        height: 22px;\n    }\n    .icon-segment i::before  {\n        color: rgb(19, 151, 219);\n    }\n    .icon-segment .line {\n        position: absolute;\n        left: 20px;\n        top: 35px;\n        z-index: 1;\n    }\n    #story-time .story .story-info {\n        /*position:absolute;\n        left: 40px;*/\n        position: relative;\n        margin-left: 40px;\n        \n        width: 80%;\n        div + div:not(.story-date) {\n            font-size: 11px;\n        }\n        &>div:first-of-type strong, .story-date {\n            font-size: 11px;\n        }\n        div {\n            text-transform: capitalize;\n        }\n        .story-type {\n            text-transform: uppercase;\n            padding-bottom:5px;\n        }\n        .story-date {\n            position:absolute;\n            right: 0px;\n            top: 0px;\n        }\n        //Styles for single line of business- SLOB\n        //when it is more than 400px with both\n        //profiler and story side-by-side\n        @media (min-width: 400px) {\n            .story-date {\n                position:static;\n            }\n        }\n        //used in the normal collapsed state when it is\n        //less than 300px for the profiler stacked on top\n        //of the story\n        @media (max-width: 300px) {\n            .story-date {\n                position:static;\n            }\n        }\n    }\n    }\n}",
            "%vlocity_namespace%__HTML__c": "<div class=\"account-story\" ng-init=\"showInfo = true\" ng-if=\"$root.entitySelected\">\n    <div class=\"spacer story-spacer\" ng-click=\"showInfo = !showInfo\">\n        Story <span ng-if=\"showInfo\"><i class=\"icon-v-down-caret\"></i></span>\n              <span ng-if=\"!showInfo\"><i class=\"icon-v-right-caret\"></i></span>\n    </div>\n    <div id=\"story-time\" ng-show=\"showInfo\">\n      <div class=\"story-type\">\n            <label>Type:</label>\n            <select ng-model=\"importedScope.selectedStory\"\n                    ng-options=\"story.objType for story in importedScope.uniqueStoryList | orderBy:story.objType\"\n                    ng-change=\"importedScope.change()\">\n                <option value=\"\">All</option>\n            </select>\n      </div>\n      <div class=\"story\" ng-repeat=\"story in importedScope.stories\" ng-click=\"importedScope.onNavigate(story.Id, story.objType, story.navigateLink)\">\n        <div class=\"icon-segment\">\n          <img src=\"{{story.imageRef}}\">\n          <div class=\"line\"></div>\n        </div>\n        <div class=\"story-info\">\n          <div class=\"story-type\"><strong>{{story.objType}}</strong></div>\n          <div>{{story.title}}</div>\n          <div class=\"story-date\">{{story.formatedLastActivityDate}}</div>\n        </div>\n      </div>\n    </div>\n</div>",
