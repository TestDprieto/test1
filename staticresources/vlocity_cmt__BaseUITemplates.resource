            "%vlocity_namespace%__DisplayOn__c": "All",
            "%vlocity_namespace%__HTML__c": "<section class=\"console-card open\" data-card=\"card-{{cardIndex}}\" ng-class=\"{selected:isSelected(cardIndex)}\" hotkey-layout-card>\n    <a href=\"#\" ng-click=\"performAction(data.actions[0])\" class=\"add-policy\" hotkey-card-action>\n        <i ng-if=\"!data.actions[0].imageRef\" class=\"icon {{data.actions[0].vlocityIcon}}\"></i>\n        <img ng-if=\"data.actions[0].imageRef\" ng-src=\"{{data.actions[0].imageRef}}\" alt=\"{{data.actions[0].displayName}}\"/>\n        <span>{{data.title}}</span>\n    </a>\n    <p class=\"description\">{{data.actions[0].displayName}}</p>\n    <ul class=\"secondary-actions\">\n        <li ng-repeat=\"action in data.actions.slice(1,actions.length)\">\n            <a href=\"#\" ng-click=\"performAction(action)\" hotkey-card-action>{{action.displayName}}</a>\n        </li>\n    </ul>\n</section>",
            "%vlocity_namespace%__Sass__c": "/*** VARIABLES ***/\n@import \"variables\";\n\n/*** MIXINS ***/\n@import \"mixin\";\n\n/*** STYLES ***/\n\n.vlocity {\n    section.console-card {\n        &.open {\n            background-color: $lightest-gray;\n            padding-top: 63px;\n            position: relative;\n\n            a.add-policy {\n                display: inline-block;\n                width: 100%;\n                margin-bottom: 8px;\n\n                i.icon {\n                    font-size: 80px;\n                    color: $light-blue;\n                    display: block;\n                    text-align: center;\n                    margin-bottom: 3px;\n                }\n\n                span {\n                    text-align: center;\n                    display: block;\n                    font-size: 30px;\n                    line-height: 30px;\n                    font-weight: 300;\n                    color: $light-blue;\n                    padding: 0 20px;\n                }\n\n                &:hover {\n                    text-decoration: none;\n                    \n                    i.icon, span {\n                        color: $dark-blue;\n                    }\n                }\n            }\n\n            p.description {\n                color: $text-gray;\n                font-size: 16px;\n                text-transform: none;\n                line-height: 22px;\n                width: 60%;\n                margin: 0 20%;\n                text-align: center;\n            }\n\n            ul.secondary-actions {\n                list-style: none;\n                padding: 0;\n                margin: 0;\n                text-align: center;\n                position: absolute;\n                bottom: 20px;\n                width: 100%;\n\n\n                li {\n                    display: inline-block;\n                    padding: 0 12px;\n                    border-right: 1px solid $middle-light-gray;\n\n                    &:last-of-type {\n                        border: none;\n                    }\n\n                    a {\n                        font-size: 12px;\n                        color: $light-blue;\n                        line-height: 16px;\n\n                        &:hover {\n                            color: $dark-blue;\n                        }\n                    }\n                }\n            }\n        }\n    }\n}",
