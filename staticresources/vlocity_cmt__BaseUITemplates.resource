    text-transform: none; }\n  .vlocity section.big-console-card.active .card-info {\n    padding: 5px 20px; }\n    .vlocity section.big-console-card.active .card-info:before, .vlocity section.big-console-card.active .card-info:after {\n      content: \" \";\n      display: table; }\n    .vlocity section.big-console-card.active .card-info:after {\n      clear: both; }\n    .vlocity section.big-console-card.active .card-info .column {\n      float: left;\n      padding: 10px 20px;\n      width: 33.33%;\n      border-right: 2px solid #f2f2f2;\n      height: 100vh;\n      overflow-y: auto; }\n      .vlocity section.big-console-card.active .card-info .column:first-child {\n        padding-left: 0px; }\n      .vlocity section.big-console-card.active .card-info .column:last-child {\n        border-right: none;\n        padding-right: 0px; }\n    .vlocity section.big-console-card.active .card-info ul {\n      list-style: none;\n      padding: 0;\n      margin: 0; }\n      .vlocity section.big-console-card.active .card-info ul.details {\n        float: left;\n        padding: 10px 5px 0px 0px;\n        width: 100%;\n        cursor: pointer; }\n        .vlocity section.big-console-card.active .card-info ul.details li {\n          margin-bottom: 16px; }\n          .vlocity section.big-console-card.active .card-info ul.details li:last-of-type {\n            margin: 0; }\n        .vlocity section.big-console-card.active .card-info ul.details span {\n          display: block; }\n          .vlocity section.big-console-card.active .card-info ul.details span.detail-label {\n            font-weight: 600;\n            font-size: 12px; }\n          .vlocity section.big-console-card.active .card-info ul.details span.value {\n            text-transform: none;\n            color: #9999a7;\n            font-size: 14px;\n            font-weight: 300; }\n      .vlocity section.big-console-card.active .card-info ul.actions {\n        float: left;\n        width: 100%; }\n        .vlocity sec