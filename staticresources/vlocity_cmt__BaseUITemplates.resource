            "%vlocity_namespace%__CSS__c": "/*** VARIABLES ***/\n.vlocity .vloc-action-grid {\n  margin-top: 60px;\n  background-color: #FFFFFF;\n  padding-right: 8px;\n  padding-top: 8px; }\n  .vlocity .vloc-action-grid:after {\n    clear: both;\n    content: '';\n    display: block; }\n  .vlocity .vloc-action-grid .vloc-action-grid-cell {\n    padding-left: 8px;\n    margin: auto;\n    min-width: 70px;\n    float: left;\n    width: 20%; }\n  .vlocity .vloc-action-grid a {\n    background-color: #f8f8f8;\n    display: block;\n    height: 100px;\n    max-width: 300px;\n    text-align: center;\n    cursor: pointer;\n    color: #05a6df;\n    margin: 0 0 8px;\n    padding: 30px 2px;\n    overflow: hidden;\n    text-decoration: none; }\n    .vlocity .vloc-action-grid a i, .vlocity .vloc-action-grid a p {\n      color: #05a6df; }\n    .vlocity .vloc-action-grid a i {\n      font-size: 24px; }\n    .vlocity .vloc-action-grid a p {\n      text-transform: capitalize; }\n    .vlocity .vloc-action-grid a:hover, .vlocity .vloc-action-grid a:active, .vlocity .vloc-action-grid a:focus {\n      background: #05a6df;\n      text-decoration: none; }\n      .vlocity .vloc-action-grid a:hover i, .vlocity .vloc-action-grid a:hover p, .vlocity .vloc-action-grid a:active i, .vlocity .vloc-action-grid a:active p, .vlocity .vloc-action-grid a:focus i, .vlocity .vloc-action-grid a:focus p {\n        color: #FFFFFF;\n        text-decoration: none; }",
