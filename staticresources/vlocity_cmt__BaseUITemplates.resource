            "%vlocity_namespace%__CSS__c": "/*** VARIABLES ***/\n/*** COMMUNITY VARIABLES ***/\n/* Breakpoints */\n/* ipad mini/ipad width is 768px */\n.vlocity .community {\n  /* Profile*/ }\n  .vlocity .community .profile {\n    border: 1px solid #dedede;\n    background-color: #ffffff; }\n    .vlocity .community .profile .content {\n      padding: 10px 15px; }\n      .vlocity .community .profile .content .welcome-note {\n        color: #9a9a9a; }\n      .vlocity .community .profile .content h2 {\n        font-size: 25px;\n        font-weight: bold;\n        margin: 5px 0px 15px; }\n      .vlocity .community .profile .content ul {\n        margin: 0;\n        padding: 0; }\n        .vlocity .community .profile .content ul.details li {\n          padding-bottom: 10px;\n          list-style-type: none; }\n          .vlocity .community .profile .content ul.details li .detail-label {\n            text-transform: uppercase;\n            margin-bottom: 0;\n            color: #9a9a9a; }\n    .vlocity .community .profile .actions ul {\n      list-style-type: none;\n      padding: 0px; }\n      .vlocity .community .profile .actions ul li {\n        padding: 10px 15px;\n        border-top: 1px solid #dedede;\n        text-transform: uppercase;\n        text-overflow: ellipsis;\n        overflow: hidden;\n        white-space: nowrap;\n        color: #9a9a9a; }\n        .vlocity .community .profile .actions ul li i {\n          color: #05A6DF;\n          font-size: 20px; }\n        .vlocity .community .profile .actions ul li .action-title {\n          line-height: 20px;\n          vertical-align: top;\n          color: #9a9a9a;\n          text-transform: uppercase; }\n  @media (max-width: 767px) {\n    .vlocity .community .profile {\n      border: none;\n      background-color: transparent; }\n      .vlocity .community .profile .content {\n        color: #ffffff; }\n        .vlocity .community .profile .content h2 {\n          font-size: 30px; }\n        .vlocity .community .profile .content .welcome-note {\n          color: #ffffff; }\n      .vlocity .community .profile .actions ul {\n        background-color: #05A6DF; }\n        .vlocity .community .profile .actions ul li {\n          padding: 10px 5px;\n          border-top: none;\n          display: inline-block;\n          text-align: center;\n          color: #ffffff;\n          width: 24%; }\n          .vlocity .community .profile .actions ul li i {\n            display: block;\n            color: #ffffff;\n            font-size: 40px; }\n          .vlocity .community .profile .actions ul li .action-title {\n            color: #ffffff; } }",
