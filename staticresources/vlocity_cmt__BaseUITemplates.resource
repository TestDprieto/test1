            "%vlocity_namespace%__Sass__c": ".vlocity {\n    .profile-box {\n        height: auto;\n        background-color: #456B8E;\n        box-sizing: border-box;\n        padding: 10px;\n        font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;\n        color: white;\n        font-size: 11px;\n        font-weight: lighter;\n        //Remove this code, temporary fix\n        * {\n            color:white;\n        }\n        .customer-type {\n            display:inline;\n            position:absolute;\n            top:5px;\n            right:15px;\n            .sentiment {\n                width:23px;\n                height:23px;\n                margin:0px 3px;\n                display:inline-block;\n                &.disgruntled {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Disgruntled.png\");\n                }\n                &.dissatisfied {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Dissatisfied.png\");\n                }\n                &.neutral {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Neutral.png\"); \n                }\n                &.promoter {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Promoter.png\");\n                }\n                &.satisfied {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Satisfied.png\");\n                }\n            }\n            .value-type {\n                width:18px;\n                height:23px;\n                margin:0px 3px;\n                display:inline-block;\n                &.bronze {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/value/Bronze.png\");\n                }\n                &.gold {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/value/Gold.png\");\n                }\n                &.platinum {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/value/Platinum.png\");\n                }\n                &.silver {\n                    background-image: url(\"{{'CustomerIcons' | resource}}/value/Silver.png\");\n                }\n            }\n        }\n        .profile-box-img {\n            align-items: center;\n            justify-content: center;\n        }\n        \n        @media (min-width: 400px) {\n        //Single Line of Business styles\n        //Sidebar Loads in iframe, media query is applicable\n            .profile-box-img {\n                display:table-cell;\n                vertical-align:top;\n                padding:8px;\n            }\n            .profile-box-details {\n                display:table-cell;\n                vertical-align:top;\n                padding-left:10px;\n                padding-top: 10px;\n                text-align:left;\n                .title-card {\n                    text-align:left;\n                }\n                .social {\n                    text-align:left;\n                }\n                .profile-links {\n                    text-align:left;\n                }\n            }\n        }\n        \n        .title-card {\n            margin: 2% auto;\n            text-align: center;\n            h1{\n                text-transform: capitalize;\n                color: white;\n                margin:10px 0 5px;\n                font-size:16px;\n            }\n        }\n        #customer-since {\n            color: white;\n            font-size: 10px;\n            font-weight: lighter;\n            text-align: center;\n        }\n\n        .profile-img  {\n            min-width:90px;\n            min-height:90px;\n            display:inline-block;\n            border: 2px solid white !important;\n        }\n        .social {\n            text-align: center;\n            margin: 15px auto;\n            color: white;\n            i {\n                cursor:pointer;\n            }\n            i::before {\n                font-size: 20px;\n            }\n            i:hover {\n                color: green;\n            }\n        }\n        .profile-links {\n            color: rgba(106, 118, 114, 1);\n            font-size: 11px;\n            text-align: center;\n            a{\n                color: white;\n                font-size:10px;\n                font-weight: lighter;\n                cursor: pointer;\n                text-transform: uppercase;\n            }\n            .separator {\n                line-height:15px;\n                color:white;\n                padding:0px 5px;\n            }\n        }\n    }\n}",
            "%vlocity_namespace%__HTML__c": "<div class=\"profile-box\" style=\"background-image:url('{{$root.staticResourceURL.consoleSidebarProfile}}');\">\n    <div class=\"customer-type\">\n        <!-- Add the sentiments and values from records -->\n        <div class=\"sentiment satisfied\"></div>\n        <div class=\"value-type platinum\"></div>\n    </div>\n    \n    <div class=\"profile-box-img\">\n        <div style=\"text-align:center;\">\n            <img id=\"user-photo\" src=\"{{records[0].PhotoUrl}}\" class=\"img-circle profile-img\"\n            ng-click=\"importedScope.openTab()\" width=\"100px\"/>\n        </div>\n    </div>\n\n    <div class=\"profile-box-details\">\n        <div class=\"title-card\">\n            <h1>{{obj.Name}}</h1>\n            <p>{{obj.Website}}</p>\n        </div>\n        <div class=\"social\">\n                <a href=\"tel:{{records[0].Phone}}\"><i class=\"icon-v-phone-line\"></i></a>\n                <a href=\"mailto:{{records[0].Email__c}}\" target=\"_top\"><i class=\"icon-v-email-line\"></i></a>\n                <i class=\"icon-v-payment-line\"></i>\n                <a href=\"sms:{{records[0].Phone}}\"><i class=\"icon-v-chat2-line\"></i></a>\n        </div>\n        <div class=\"profile-links\">\n            <a href=\"#\" id=\"profile-edit\" >EDIT PROFILE</a> <span class=\"separator\">|</span><a href=\"#\">VERIFY CALLER</a> <span class=\"separator\">|</span> <a href=\"#\">RESET PASSWORD</a>\n        </div>\n    </div>\n</div>",
            "%vlocity_namespace%__DisplayOn__c": "All",
            "%vlocity_namespace%__CSS__c": ".vlocity .profile-box {\n  height: auto;\n  background-color: #456B8E;\n  box-sizing: border-box;\n  padding: 10px;\n  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;\n  color: white;\n  font-size: 11px;\n  font-weight: lighter; }\n  .vlocity .profile-box * {\n    color: white; }\n  .vlocity .profile-box .customer-type {\n    display: inline;\n    position: absolute;\n    top: 5px;\n    right: 15px; }\n    .vlocity .profile-box .customer-type .sentiment {\n      width: 23px;\n      height: 23px;\n      margin: 0px 3px;\n      display: inline-block; }\n      .vlocity .profile-box .customer-type .sentiment.disgruntled {\n        background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Disgruntled.png\"); }\n      .vlocity .profile-box .customer-type .sentiment.dissatisfied {\n        background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Dissatisfied.png\"); }\n      .vlocity .profile-box .customer-type .sentiment.neutral {\n        background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Neutral.png\"); }\n      .vlocity .profile-box .customer-type .sentiment.promoter {\n        background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Promoter.png\"); }\n      .vlocity .profile-box .customer-type .sentiment.satisfied {\n        background-image: url(\"{{'CustomerIcons' | resource}}/sentiment/Satisfied.png\"); }\n    .vlocity .profile-box .customer-type .value-type {\n      width: 18px;\n      height: 23px;\n      margin: 0px 3px;\n      display: inline-block; }\n      .vlocity .profile-box .customer-type .value-type.bronze {\n        background-image: url(\"{{'CustomerIcons' | resource}}/value/Bronze.png\"); }\n      .vlocity .profile-box .customer-type .value-type.gold {\n        background-image: url(\"{{'CustomerIcons' | resource}}/value/Gold.png\"); }\n      .vlocity .profile-box .customer-type .value-type.platinum {\n        background-image: url(\"{{'CustomerIcons' | resource}}/value/Platinum.png\"); }\n      .vlocity .profile-box .customer-type .value-type.silver {\n        background-image: url(\"{{'CustomerIcons' | resource}}/value/Silver.png\"); }\n  .vlocity .profile-box .profile-box-img {\n    align-items: center;\n    justify-content: center; }\n  @media (min-width: 400px) {\n    .vlocity .profile-box .profile-box-img {\n      display: table-cell;\n      vertical-align: top;\n      padding: 8px; }\n    .vlocity .profile-box .profile-box-details {\n      display: table-cell;\n      vertical-align: top;\n      padding-left: 10px;\n      padding-top: 10px;\n      text-align: left; }\n      .vlocity .profile-box .profile-box-details .title-card {\n        text-align: left; }\n      .vlocity .profile-box .profile-box-details .social {\n        text-align: left; }\n      .vlocity .profile-box .profile-box-details .profile-links {\n        text-align: left; } }\n  .vlocity .profile-box .title-card {\n    margin: 2% auto;\n    text-align: center; }\n    .vlocity .profile-box .title-card h1 {\n      text-transform: capitalize;\n      color: white;\n      margin: 10px 0 5px;\n      font-size: 16px; }\n  .vlocity .profile-box #customer-since {\n    color: white;\n    font-size: 10px;\n    font-weight: lighter;\n    text-align: center; }\n  .vlocity .profile-box .profile-img {\n    min-width: 90px;\n    min-height: 90px;\n    display: inline-block;\n    border: 2px solid white !important; }\n  .vlocity .profile-box .social {\n    text-align: center;\n    margin: 15px auto;\n    color: white; }\n    .vlocity .profile-box .social i {\n      cursor: pointer; }\n    .vlocity .profile-box .social i::before {\n      font-size: 20px; }\n    .vlocity .profile-box .social i:hover {\n      color: green; }\n  .vlocity .profile-box .profile-links {\n    color: #6a7672;\n    font-size: 11px;\n    text-align: center; }\n    .vlocity .profile-box .profile-links a {\n      color: white;\n      font-size: 10px;\n      font-weight: lighter;\n      cursor: pointer;\n      text-transform: uppercase; }\n    .vlocity .profile-box .profile-links .separator {\n      line-height: 15px;\n      color: white;\n      padding: 0px 5px; }",
