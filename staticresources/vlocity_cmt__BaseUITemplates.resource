            "%vlocity_namespace%__HTML__c": "<div class=\"console-account-info\" ng-init=\"showInfo = true\">\n    <div class=\"spacer acct-spacer\" ng-click=\"showInfo = !showInfo\">\n        Account <span ng-if=\"showInfo\"><i class=\"icon-v-down-caret\"></i></span>\n                <span ng-if=\"!showInfo\"><i class=\"icon-v-right-caret\"></i></span>\n    </div>\n    <div class=\"acct-info\" ng-show=\"showInfo\">\n        <table>\n            <tr>\n                <td><strong>Address:</strong></td> <td>{{records[0].BillingAddress.street}}<br/>{{records[0].BillingAddress.city}} {{records[0].BillingAddress.state}} {{records[0].BillingAddress.postalCode}}</td>\n            </tr>\n            <tr>\n                <td><strong>Phone:</strong></td> <td>{{records[0].Phone}}</td>\n            </tr>\n        </table>\n        <div class=\"acct-links\">\n            <ul>\n                <li ng-repeat=\"action in data.actions | limitTo: (data.limitActionLinksTo?data.limitActionLinksTo:3)\">\n                    <a href=\"#\" ng-click=\"performAction(action)\">{{action.displayName}}</a>\n                    <span ng-if=\"!$last\" class=\"separator\">|</span>\n                </li>\n            </ul>\n        </div>\n    </div>\n</div>",
