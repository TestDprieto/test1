            "%vlocity_namespace%__DisplayOn__c": "All",
            "%vlocity_namespace%__HTML__c": "<div class=\"item-card\">\n    <div class=\"item-name\">{{obj.Name}}</div>\n    <ul>\n        <li ng-repeat=\"field in data.fields\" ng-show=\"key != 'attributes'\">\n            <label>{{field.label}}</label>\n            <span class=\"value\" ng-if=\"!obj[field.relationshipName]\">\n                {{obj | getter: field | picker: field.type}}\n            </span>\n            <span class=\"value\" ng-if=\"obj[field.relationshipName]\">{{obj[field.relationshipName]['Name']}}</span>\n        </li>\n    </ul>\n    <div class=\"actions\">\n        <span ng-repeat=\"action in data.actions | limitTo: (data.limitActionLinksTo?data.limitActionLinksTo:5)\">\n             <a href=\"#\" ng-click=\"performAction(action)\" hotkey-card-action>{{action.displayName}}</a> <span class=\"separator\" ng-show=\"!$last\">|</span>\n        </span>\n    </div>\n</div>",
