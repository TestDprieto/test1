st .vloc-attrs-picklist-label {\n    margin: 0; }\n    .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-label.vloc-attrs-add-new {\n      line-height: .5; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-values .vloc-ins-rules-box {\n    display: none; }\n    .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-values .vloc-ins-rules-box.showRules {\n      display: block; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-values .vloc-rules-icon.has-rules {\n    color: #0070D2;\n    fill: #0070D2; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-values .vloc-attrs-delete {\n    position: relative; }\n    .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-values .vloc-attrs-delete:hover {\n      cursor: pointer; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-picklist-values .vloc-delete-icon {\n    position: absolute;\n    right: 5px; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-add-row {\n    position: relative; }\n    .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-add-row .vloc-attrs-add-value {\n      position: absolute;\n      right: -1rem;\n      top: 0rem; }\n      .via-slds .vloc-attr-config-row .vloc-attrs-picklist .vloc-attrs-add-row .vloc-attrs-add-value:focus {\n        box-shadow: none; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-slider .vloc-attrs-slider-label {\n    margin-right: 0;\n    margin-top: 0; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-add-value:hover {\n    cursor: pointer; }\n  .via-slds .vloc-attr-config-row .vloc-attr-title-cell {\n    margin-bottom: 0; }\n  .via-slds .vloc-attr-config-row .vloc-attrs-checkbox {\n    position: absolute;\n    bottom: 1.25rem; }\n","%vlocity_namespace%__Definition__c":"","%vlocity_namespace%__Description__c":"","%vlocity_namespace%__DisplayOn__c":"","%vlocity_namespace%__ExternalID__c":"clm-runtime-new-term-config/1/Vlocity/1545175464951","%vlocity_namespace%__HTML__c":"<div class=\"vloc-attr-config-grid slds-grid slds-wrap slds-p-bottom_small slds-p-horizontal_x-small\" ng-init=\"$rootScope.index = cardIndex;importedScope.newAttr()\">\n    <!--First two Card Fields: --> \n    <div class=\"vloc-attr-config-row slds-small-show slds-p-top_small slds-size_1-of-2\" ng-repeat=\"field in data.fields.slice(0, 2)\"  >\n        <div data.fields class=\"vloc-attr-title-cell slds-truncate slds-form-element__label slds-p-right_x-small\" title=\"{{field.label}}\"> \n            <!--<span ng-if=\"$root.config.fieldSet[field.name]\">{{$root.config.fieldSet[field.name].label}}</span> -->\n            <span>{{field.label}}</span>\n        </div>\n        <div class=\"slds-size_1-of-1 slds-p-right_x-small\">\n            <input id=\"product-term-name-field\" type=\"text\" class=\"slds-input ng-pristine ng-untouched ng-valid ng-not-empty\"  title=\"{{field.label}}\" ng-model=\"$root.config.product[field.name]\" >\n        </div>\n        <!-- <input class=\"slds-input vloc-attrs-input\" type=\"{{$root.config.fieldSet[field.name].dataType}}\" ng-model=\"$root.config.attr[field.name]\" ng-if=\"$root.config.fieldSet[field.name].dataType !== 'BOOLEAN'\"  ng-class=\"{'disabled': $root.config.fieldSet[field.name].isUpdateable === false}\" ng-disabled=\"$root.config.fieldSet[field.name].isUpdateable === false\" /> -->\n\n        <div class=\"slds-form-element__control slds-text-align_left vloc-attrs-checkbox\" ng-if=\"field.dataType === 'BOOLEAN'\">\n            <span class=\"slds-checkbox\">\n                <input type=\"checkbox\" name=\"attrs-{{$index}}\" id=\"attrs-{{$index}}\" ng-model=\"$root.config.attr[field.name]\" ng-disabled=\"!$root.config.fieldSet[field.name].isUpdateable\"/>\n                <label class=\"slds-checkbox__label slds-m-around_none\" for=\"attrs-{{$index}}\">\n                    <span class=\"slds-checkbox_faux vloc-check\"></span>\n                </label>\n            </span>\n        </div>\n    </div>\n    \n    <!--Control and Value Fields: 3, 4-->\n    <div class=\"vloc-attr-config-row  vloc-attr-config-row_value slds-small-show slds-p-top_small slds-size_1-of-1\" ng-repeat=\"field in data.fields.slice(2, 4)\" ng-show=\"key != 'attributes'\"  >\n        \n        <div class=\"vloc-attr-title-cell slds-truncate slds-form-element__label slds-size_1-of-1\" title=\"{{field.label}}\"  ng-if=\"field.name === $root.nsPrefix + 'ValueDataType__c'\"><span>{{field.label}}</span></div>\n        <!--Control -->\n        <div class=\"vloc-attrs-control slds-grid slds-wrap\" ng-if=\"field.name === $root.nsPrefix + 'ValueDataType__c'\">\n           <!-- Normal Select --> \n            <div ng-if=\"!$root.config.attr[$root.nsPrefix + 'IsConfigurable__c']\" class=\"slds-size_1-of-1\">\n                 <select ng-options=\"control for control in importedScope.configurableTypeListStandard\" ng-model=\"$root.config.attr[field.name]\" class=\"slds-select vloc-attrs-select\" id=\"select-01\" ng-change=\"importedScope.setDataType(field)\"  ng-class=\"{'vloc-error-input':  $root.notification.message === 'Attribute must have data type assignment'}\">\n                </select>\n            </div>\n\n            <!--Customizable Select --> \n            <div ng-if=\"$root.config.attr[$root.nsPrefix + 'IsConfigurable__c']\" class=\"slds-size_1-of-1 slds-grid slds-wrap\">\n                <select ng-options=\"control for control in importedScope.configurableTypeListCustomizable\" ng-model=\"$root.config.attr[field.name]\" class=\"slds-select vloc-attrs-select slds-size_1-of-1\"  ng-class=\"{'slds-size_1-of-1' : importedScope.configurableTypeDict[$root.config.attr[field.name]].subType, 'vloc-error-input':  $root.notification.message === 'Attribute must have data type assignment'}\" id=\"select-01\" ng-change=\"importedScope.setDataType(field);\" >\n                </select>\n                <div  ng-if=\"importedScope.configurableTypeDict[$root.config.attr[field.name]].subType\" class=\"slds-p-left_xx-small slds-size_1-of-1\" ng-hide=\"true\">\n                    <select ng-options=\"displayType for displayType in importedScope.configurableTypeDict[$root.config.attr[field.name]].displayType\" ng-model=\"$root.config.attr[$root.nsPrefix + 'UIDisplayType__c']\" type=\"importedScope.configurableTypeDict[displayType]\" ng-change=\"importedScope.setDisplayType(displayType)\" class=\"slds-select vloc-attrs-select\" id=\"select-01\">\n                </select>\n                </div>\n            </div>\n\n        </div>\n\n        <!--Values --> \n        <div class=\"vloc-attrs-value slds-size_1-of-1\"  ng-class=\"{'slds-size_1-of-1' : importedScope.configurableTypeDict[$root.config.attr[field.name]].subType}\" ng-if=\"field.name !== $root.nsPrefix + 'ValueDataType__c'\">\n            <!--general input --> \n            <div class=\"vloc-attrs-value-general\">\n                <div class=\"vloc-attr-title-cell slds-truncate slds-form-element__label slds-size_1-of-1\" title=\"{{field.label}}\"> <span>{{field.label}}</span></div>\n                <input  ng-if=\"$root.config.attr[$root.nsPrefix + 'ValueDataType__c'] !== 'Number' && $root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] !== 'Slider' && \n                $root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] !== 'Equalizer' &&\n                $root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] !== 'Dropdown' && $root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] !== 'Checkbox' && $root.config.attr[$root.nsPrefix + 'ValueDataType__c'] !== 'Checkbox' && $root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] !== 'Radiobutton' && \n                $root.config.attr[$root.nsPrefix + 'ValueDataType__c'] !== 'Date' && \n                $root.config.attr[$root.nsPrefix + 'ValueDataType__c'] !== 'Datetime'\" class=\"slds-input\" type=\"{{$root.config.attr.inputDisplayType}}\" ng-model=\"$root.config.attr[field.name]\" ng-class=\"{'disabled': $root.config.fieldSet[field.name].isUpdateable === false}\"  ng-disabled=\"$root.config.fieldSet[field.name].isUpdateable === false\"/>\n\n                 <input ng-init=\"importedScope.toNumber($root.config.attr[field.name])\" ng-if=\"$root.config.attr[$root.nsPrefix + 'ValueDataType__c'] === 'Number'\" class=\"slds-input\" type=\"{{$root.config.attr.inputDisplayType}}\" ng-model=\"$root.config.attr[field.name]\" ng-class=\"{'disabled': $root.config.fieldSet[field.name].isUpdateable === false}\"  ng-disabled=\"$root.config.fieldSet[field.name].isUpdateable === false\"/>\n\n                <!--date picker--> \n                <div class=\"slds-form-element slds-dropdown-trigger slds-dropdown-trigger_click slds-is-open slds-grid slds-wrap\" ng-if=\"$root.config.attr[$root.nsPrefix + 'ValueDataType__c'] === 'Date'\">\n                    <input class=\"slds-input slds-m-bottom_small\" slds-date-picker ng-model=\"$root.config.attr[field.name]\" >\n                </div>\n\n                  <!--datetime picker--> \n                <div class=\"slds-form-element slds-dropdown-trigger slds-dropdown-trigger_click slds-grid slds-wrap\" ng-if=\"$root.config.attr[$root.nsPrefix + 'ValueDataType__c'] === 'Datetime'\">\n                    <div class=\"slds-m-bottom_small slds-size_1-of-2 slds-p-right_small\"> <input class=\"slds-input\" slds-date-picker ng-model=\"$root.config.attr[field.name]\"> </div>\n                     <div class=\"slds-m-bottom_small slds-size_1-of-2\"> <input class=\"slds-input \" slds-time-picker ng-model=\"$root.config.attr[field.name]\"> </div>\n                </div>\n\n                <!--slider -->\n                <div ng-if=\"$root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] === 'Slider' && $root.config.attr[$root.nsPrefix + 'IsConfigurable__c']\" class=\"vloc-attrs-slider slds-grid slds-wrap slds-m-bottom_small\" ng-init=\"importedScope.toNumber($root.config.attr[field.name])\">\n                    \n                    <input class=\"slds-input slds-size_1-of-3\"  type=\"number\" ng-model=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].min\" placeholder=\"Enter Min\"/>\n                    \n                    <input class=\"slds-input slds-size_1-of-3\" type=\"number\" ng-model=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].max\" placeholder=\"Enter Max\"/>\n                    \n                    <input  name=\"startValue\" class=\"slds-input slds-size_1-of-3\" type=\"number\" ng-model=\"$root.config.attr[field.name]\" ng-min=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].min\" ng-max=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].max\"\n                    placeholder=\"Enter Start Value\" ng-disabled=\"$root.config.fieldSet[field.name].isUpdateable === false\" ng-class=\" {'disabled': $root.config.fieldSet[field.name].isUpdateable === false}\"/>\n                    \n                    <div class=\"vloc-attrs-slider-label slds-form-element__label slds-size_1-of-3\">Min</div>\n                    <div class=\"vloc-attrs-slider-label slds-form-element__label slds-size_1-of-3\">Max</div>\n                    <div class=\"vloc-attrs-slider-label slds-form-element__label slds-size_1-of-3\">Start Value</div>\n                </div>\n\n                <div ng-if=\"$root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] === 'Equalizer' && $root.config.attr[$root.nsPrefix + 'IsConfigurable__c']\" class=\"vloc-attrs-slider slds-grid slds-wrap slds-m-bottom_small\" ng-init=\"importedScope.toNumber($root.config.attr[field.name])\">\n                    <input class=\"slds-input slds-size_1-of-2\"  type=\"number\" ng-model=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].max\" placeholder=\"Enter Variant\"/>\n                    <input class=\"slds-input  slds-size_1-of-2\" type=\"number\" ng-model=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].step\" placeholder=\"Enter Step\"/>\n                    <div class=\"vloc-attrs-slider-label slds-form-element__label slds-size_1-of-2\">Variant</div>\n                    <div class=\"vloc-attrs-slider-label slds-form-element__label slds-size_1-of-2\">Increment</div>\n                </div>\n\n                <!--picklists (Picklist, multi-picklist, dropdown)-->\n                 <div ng-if=\"$root.config.attr[$root.nsPrefix + 'IsConfigurable__c'] && ($root.config.attr.valueType === 'picklist' || $root.config.attr[$root.nsPrefix + 'ValueDataType__c'] === 'Multi Picklist' || $root.config.attr[$root.nsPrefix + 'UIDisplayType__c'] === 'Dropdown')\" class=\"vloc-attrs-picklist slds-grid slds-wrap slds-m-bottom_small\">\n                    <div class=\"slds-grid slds-wrap slds-size_1-of-1\" ng-if=\"$root.config.attr[$root.nsPrefix + 'ValidValuesData__c'].length > 0\">\n                        <div class=\"vloc-attrs-picklist-label slds-form-element__label slds-size_1-of-6\" >Default</div>\n                        <div class=\"vloc-attrs-picklist-label slds-form-element__label slds-m-right_xx-small slds-size_1-of-3\" >Display Value</div>\n                        <div class=\"slds-p-left_xx-small slds-form-element__label slds-size_1-of-3\" >Value</div>\n                        <div class=\"vloc-attrs-picklist-label slds-form-element__label slds-size_1-of-6\" ></div>\n                    </div>\n                    <fieldset class=\"slds-form-element slds-size_1-of-1\" >\n                    <!--Values: --> \n                    <div class=\"vloc-attrs-picklist-values slds-m-bottom_medium\">\n                        <fieldset>\n                            <div class=\"slds-form-element__control slds-grid slds-wrap slds-size_1-of-1 \" ng-repeat=\"option in $root.config.attr[$root.nsPrefix + 'ValidValuesData__c']\" ng-class=\"{'slds-box option-show-rule': option.showRules}\">\n                                <div class=\"slds-size_1-of-6 slds-p-left_small\" ng-init=\"option.id = $index\">\n                                    <span class=\"sl