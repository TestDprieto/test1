            "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n/*** STYLES ***/\n.vlocity {\n    .cpq-product-cart-item-addon-same-section {\n        \n        .cpq-cart-item-attr-value {\n            font-weight: $cpq-headings-font-weight;\n            color: $cpq-product-cart-items-attr-value-color;\n        }\n\n        .cpq-cart-item-input {\n            padding: 0 3px;\n            margin-top: $cpq-padding-large-vertical/5;\n            text-align: center;\n            width: 50px; //width can fit 3 digits\n            height: 25px;\n            min-height: calc(25px + 2px); //override slds input styles\n        }\n        .cpq-line-item-container {\n            max-width: 100%;\n        }\n        \n        .cpq-component-content {\n            display: none;\n        }\n\n        .slds-section.slds-is-open .cpq-component-content {\n            display: block;\n        }\n        \n        .cpq-cart-promo-item {\n            background-color: $cpq-cart-promo-item-bg-color;\n            border-style: solid;\n            border-radius: $cpq-cart-promo-item-border-radius;\n            border-width: $cpq-cart-promo-item-border-width;\n            border-color: $cpq-cart-promo-item-border-color;\n            margin-left: $cpq-padding-small-horizontal * 2;\n            padding: $cpq-padding-large-horizontal;\n            \n            .u-Flex-childProduct {\n                flex: 0 0 240px;\n            }\n            \n            .add-button {\n                background-color: $cpq-cart-promo-button-bg-color;\n                border: 0px;\n            }\n        }\n        \n        .fix-slds-close-switch {\n            transform: rotate(-90deg) !important;\n        }\n        \n    }\n    \n    .cpq-product-cart-item-section-heading {\n        font-size: $cpq-cart-section-title-font-size;\n    }\n    \n    .cpq-cmp-config-panel {\n        padding-left: 25px;\n    }\n}",
