            "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-product-item-details {\n        .cpq-product-item-details-gallery {\n            margin-top: $cpq-padding-large-vertical*2;\n            ul {\n                height: 140px;\n                overflow-x: auto;\n                white-space: nowrap;\n                padding: $cpq-padding-large-vertical 0px;\n                li {\n                    width: 120px;\n                    height: 100px;\n                    padding: $cpq-padding-small-horizontal;\n                    margin-right: $cpq-padding-small-horizontal;\n                    display: inline-block;\n                    border: 1px solid $middle-light-gray;\n                    vertical-align: middle;\n                    text-align: center;\n                    &:hover{\n                        img {\n                            transform: scale(1.5,1.5);\n                            transition: .3s transform;\n                        }\n                    }\n                    img{\n                        max-width: 100%;\n                        height: auto;\n                        transform: translateZ(0);\n                    }\n                }\n            }\n        }\n        .cpq-product-item-details-features {\n            margin-top: $cpq-padding-large-vertical*2;\n        }\n        .cpq-product-item-details-form {\n            max-width: 400px;\n        }\n        .cpq-product-item-details-addon {\n            position: relative; //needed for selected border\n            .cpq-product-item-title {\n                font-weight: $cpq-headings-font-weight;\n            }\n            .cpq-product-item-details-addon-details {\n                ul {\n                    text-align: center;\n                    li {\n                        display: inline-block;\n                        margin: 0 $cpq-padding-large-horizontal/2;\n                        vertical-align: top;\n                        .cpq-product-item-attr-value {\n                            font-weight: $cpq-headings-font-weight;\n                            color: $cpq-product-cart-items-attr-value-color;\n                        }\n                        .cpq-product-item-attr-label {\n                            font-size: $cpq-cart-item-attr-label-font-size;\n                            text-transform: uppercase;\n                        }\n                    }\n                }\n            }\n        }\n        .cpq-product-details-qty-input {\n            padding: 0 3px;\n            margin-top: $cpq-padding-large-vertical/5;\n            text-align: center;\n            width: 50px; //width can fit 3 digits\n            height: 25px;\n            min-height: calc(25px + 2px); //override slds input styles\n        }\n        .slds-tree_container {\n            max-width: 100%; //overwrite Salesforce tree max width 400px\n        }\n        .slds-grid {\n            width: 100%;\n        }\n        //@TODO Rename the classes and logic needs to be revisited\n        .Table {\n            display: flex;\n            flex-flow: column nowrap;\n            box-pack: justify;\n            justify-content: space-between;\n            // border: 1px solid #f2f2f2;\n            // font-size: 1rem;\n            // margin: 0.5rem;\n            // line-height: 1.5;\n        }\n        .Table-header {\n          font-weight: bold;\n        }\n        .Table-row {\n          display: flex;\n          flex-flow: row nowrap;\n          flex: 1 1 auto;\n        }\n\n        .Table-row-item {\n          display: flex;\n          flex-flow: row nowrap;\n          flex: 1 0 auto;\n          word-wrap: break-word;\n          overflow-wrap: break-word;\n        /*   padding: 10px; */\n        }\n\n        .u-Flex-icon {\n            flex: 0 0 25px;\n        }\n\n        .u-Flex-product {\n          flex: