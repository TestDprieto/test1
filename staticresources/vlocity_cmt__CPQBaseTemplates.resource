            "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-product-item {\n        border-bottom: $cpq-product-item-padding solid $cpq-page-bg-color;\n        \n        .cpq-product-item-container {\n            padding: $cpq-padding-large-vertical 0px;\n            margin: 0px $cpq-padding-large-horizontal;\n        \n            .cpq-product-item-select-checkbox {\n                // Ensure atleast 40px width for inputs for device touch area\n                padding: $cpq-padding-small-horizontal;\n                margin-top: -$cpq-padding-small-horizontal/2;\n            }\n            \n            .product-name {\n                font-weight: $cpq-headings-font-weight;\n                line-height: 30px;\n            }\n            \n            .product-link {\n                line-height: 30px;\n                color: #0070d2;\n            }\n            .product-price div {\n                padding-right: $cpq-padding-large-horizontal;\n                padding-top:$cpq-padding-large-vertical;\n                color: $dark-gray;\n                &:last-child {\n                    padding-bottom:$cpq-padding-large-vertical;\n                }\n                \n                .price-label {\n                  font-size: $cpq-cart-item-attr-label-font-size;\n                }\n            }\n            \n            .offers-count {\n                background-color: #FDB734;\n                color: white;\n                margin-left: 5px;\n            }\n        }\n        \n        .cpq-product-actions {\n            padding-top:$cpq-padding-large-vertical;\n            padding-bottom:$cpq-padding-large-vertical; \n            \n            .add-button {\n                padding-left:$cpq-padding-small-horizontal;\n                padding-right:$cpq-padding-small-horizontal;\n           }\n        }\n    }\n}",
