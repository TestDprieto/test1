            "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-product-cart {\n        position: absolute;\n        top: 0;\n        right: $cpq-padding-large-horizontal/2;\n        bottom: 0;\n        left: $cpq-padding-large-horizontal/2;\n        height: calc(100% - 80px);\n        \n        .cart-errors {\n            padding: $cpq-padding-large-vertical $cpq-padding-large-horizontal;\n            color: white;\n            h2 {\n                font-size: 14px;\n            }\n            a {\n                text-decoration: none;\n            }\n            \n             button {\n                color: white;\n            }\n        }\n    }\n    .cpq-product-cart-tabs {\n        margin-top: $cpq-cart-container-tabs-top-margin;\n    }\n    .cpq-product-cart-items {\n        background-color: $cpq-product-cart-items-bg-color;\n        overflow-y: auto;\n        .cpq-product-cart-items-tab-content {\n            padding-top: 0px; //slds override\n            .cpq-no-cart-items-msg {\n                padding: $cpq-padding-large-vertical*4 $cpq-padding-large-vertical;\n            }\n        }\n        .next-product {\n            padding: $cpq-padding-large-vertical;   \n         \n            .slds-spinner_container {\n                position: relative;\n                padding-top: 10px;\n            }\n        }\n    }\n    \n    .fix-slds-close-switch {\n        transform: rotate(-90deg);    \n    }\n    \n}",
            "%vlocity_namespace%__HTML__c": "<div class=\"slds-grid slds-grid--vertical cpq-product-cart\">\n    <div class=\"slds-spinner_container\" ng-show=\"!isLoaded\">\n        <div class=\"slds-spinner--brand slds-spinner slds-spinner--medium\" aria-hidden=\"false\" role=\"alert\">\n            <div class=\"slds-spinner__dot-a\"></div>\n            <div class=\"slds-spinner__dot-b\"></div>\n        </div>\n    </div>\n    <div class=\"slds-col slds-no-flex cpq-product-cart-tabs\">\n        <ul class=\"slds-tabs--default__nav\" role=\"tablist\">\n            <li class=\"slds-tabs--default__item slds-text-heading--label slds-active\" title=\"Item One\" role=\"presentation\"><a class=\"slds-tabs--default__link\" href=\"#voi\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"tab-default-1\" id=\"tab-default-1__item\">Cart</a></li>\n        </ul>\n    </div>\n    \n    <!--The scroll Class is used for mobile devices -->\n    <div class=\"slds-col cpq-product-cart-items scroll\">\n        <!--SHOW CART ERRORS-->\n        <div class=\"\" ng-if=\"session.messages.length > 0\">\n            \n            <!--<div class=\"slds-notify__content slds-grid\" ng-if=\"session.messages.length > 1\">-->\n            <!--    <div class=\"slds-col slds-align-middle\"  ng-click=\"showMessages = !showMessages\">-->\n            <!--        <h2 class=\"slds-text-heading--small\"> Messages ({{session.messages.length}}) {{showMessages}}</h2>-->\n            <!--    </div>-->\n            <!--</div>-->\n            \n            <div class=\"slds-theme--error cart-errors slds-section__title\" ng-show=\"session.messages.length > 1\">\n                <a href=\"javascript:void(0);\" ng-click=\"showMessages = !showMessages\" class=\"slds-section__title-action cpq-product-cart-item-section-heading\">\n                    <button class=\"slds-button slds-button--icon-container slds-button--icon-x-small\">\n                        <svg aria-hidden=\"true\" class=\"slds-button__icon\" ng-class=\"{'fix-slds-close-switch' : !showMessages}\">\n                          <use xlink:href=\"{{'/assets/icons/utility-sprite/svg/symbols.svg#switch' | sldsStaticResourceURL}}\"></use>\n                        </svg>\n                    </button>\n                    Messages ({{session.messages.length}})\n                </a>\n            </div>\n            \n            <div class=\"slds-notify cart-errors\" role=\"alert\" ng-repeat=\"msg in session.messages\" ng-show=\"(session.messages.length == 1 && records.length > 0) || showMessages\" ng-class=\"{'slds-theme--error': msg.severity == 'ERROR', 'slds-theme--warning': msg.severity == 'WARNING', 'slds-theme--alert': msg.severity == 'INFO'}\">\n                <div class=\"slds-notify__content slds-grid\">\n                    <div class=\"slds-col slds-align-middle\" >\n                        <h2 class=\"slds-text-heading--small\">{{msg.message}}</h2>\n                    </div>\n                    <button class=\"slds-button slds-p-horizontal--xx-small\" ng-click=\"importedScope.openDetailView(msg)\" ng-if=\"msg.actions['DETAILS']\">\n                        <svg aria-hidden=\"true\" class=\"slds-button__icon\">\n                            <use xlink:href=\"{{'/assets/icons/utility-sprite/svg/symbols.svg#preview' | sldsStaticResourceURL}}\"></use>\n                        </svg>\n                        <span class=\"slds-assistive-text\">Details</span>\n                    </button>\n                </div>\n            </div>\n        </div>\n        <!--END CART ERRORS-->\n        <div class=\"slds-tabs--default\">\n            <div id=\"tab-default-1\" class=\"slds-tabs--default__content cpq-product-cart-items-tab-content slds-show\" role=\"tabpanel\" aria-labelledby=\"tab-default-1__item\">\n                \n                <vloc-card ng-repeat=\"card in cards\" ctrl=\"CPQCartItemController\" ng-model=\"card\" data=\"card\" records=\"records\" index=\"{{$index}}\" ng-if=\"isLoaded\"></vloc-card>\n                <div class=\"slds-grid slds-grid--vertical-align-center slds-grid--align-center cpq-no-cart-items-msg\" ng-if=\"records.length === 0\">\n                    Cart is empty.\n                </div>\n                 <div class=\"next-product slds-col slds-text-align--center slds-m-vertical--medium\" ng-if=\"session.nextProducts\">\n            <div class=\"slds-spinner_container\" ng-show=\"loadingMore\">\n                <div class=\"slds-spinner--brand slds-spinner slds-spinner--small\" aria-hidden=\"false\" role=\"alert\">\n                    <div class=\"slds-spinner__dot-a\"></div>\n                    <div class=\"slds-spinner__dot-b\"></div>\n                </div>\n            </div>\n            <a ng-click=\"importedScope.nextPage()\" ng-if=\"!loadingMore\">\n              Load More\n            </a>\n \n        </div>\n            </div>\n            <div id=\"tab-default-2\" class=\"slds-tabs--default__content cpq-product-cart-items-tab-content slds-hide\" role=\"tabpanel\" aria-labelledby=\"tab-default-2__item\">Tab Two Content</div>\n            <div id=\"tab-default-3\" class=\"slds-tabs--default__content cpq-product-cart-items-tab-content slds-hide\" role=\"tabpanel\" aria-labelledby=\"tab-default-3__item\">Tab Three Content</div>\n        </div>\n    </div>\n    <!--<vloc-layout layout-name=\"cpq-total-bar\" ctrl=\"CPQTotalController\" class=\"cpq-product-cart-order\"></vloc-layout>-->\n</div>",
