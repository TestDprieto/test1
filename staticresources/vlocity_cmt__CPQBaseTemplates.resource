            "%vlocity_namespace%__HTML__c": "<div class=\"cpq-product-filters slds-grid slds-grid--vertical\">\n    <!--<pre> {{records | json}}</pre>-->\n    <!-- scroll class is added for ionic scroll-->\n    <div class=\"slds-col cpq-product-cart-config-form scroll\">\n        <!-- form -->\n        <div class=\"\">\n            <button type=\"button\" class=\"slds-button\" ng-click=\"importedScope.resetFilter(records)\">{{ ::'ResetFilters' | localize: 'Reset' }}\n            </button>\n            <button type=\"button\" class=\"slds-button slds-button--neutral\" ng-click=\"importedScope.submitFilter(records)\">{{::'ResetFilters' | localize: 'Apply' }}</button>\n        </div>\n        \n        <div ng-if=\"records.length >0\">\n            <vlocity-dynamic-form ng-model=\"records\" map-object=\"{{importedScope.filterMapObject()}}\" class=\"slds-form--stacked\"></vlocity-dynamic-form>\n        </div>\n        \n        <div ng-if=\"records.length === 0\">\n            No filters are available for this list.\n        </div>\n    </div>\n</div>",
