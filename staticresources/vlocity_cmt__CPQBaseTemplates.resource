            "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-products {\n        position: absolute;\n        top: 0;\n        right: $cpq-padding-large-horizontal/2;\n        bottom: 0;\n        left: $cpq-padding-large-horizontal/2;\n    }\n    \n    .cpq-product-searchbox {\n        vertical-align: middle;\n        margin-bottom: $cpq-padding-large-vertical;\n        \n        input {\n           background-color: $cpq-product-container-bg-color;\n           border-radius: 0px;\n           border: 1px solid $cpq-product-container-bg-color;;\n        }\n    }\n    \n    .cpq-product-filter {\n        background-color: $cpq-product-container-bg-color;\n        margin-bottom: $cpq-product-item-padding;\n        section {\n            padding: $cpq-padding-base-vertical 0px;\n            margin: 0px $cpq-padding-large-horizontal;\n        }\n        \n        a {\n            text-decoration: none;\n            color: $mid-black;\n            \n            &:hover {\n                color:$dark-blue;\n            }\n        }\n    }\n    \n    .cpq-items-container {\n        overflow-y: scroll;\n        -webkit-overflow-scrolling: touch;\n         background-color: $cpq-product-container-bg-color;\n         \n        .cpq-no-products-msg {\n            padding: $cpq-padding-large-vertical;\n        }\n    }\n    \n    .next-product {\n         padding: $cpq-padding-large-vertical;   \n         \n         .slds-spinner_container {\n            position: relative;\n            padding-top: 10px;\n         }\n    }\n}",
            "%vlocity_namespace%__HTML__c": "<div class=\"slds-grid slds-grid--vertical cpq-products scroll\">\n    <div class=\"slds-col slds-no-flex slds-input-has-icon slds-input-has-icon--left cpq-product-searchbox\">\n        <svg aria-hidden=\"true\" class=\"slds-icon slds-input__icon slds-icon--x-small slds-icon-text-default\">\n            <use xlink:href=\"{{'/assets/icons/utility-sprite/svg/symbols.svg#search' | sldsStaticResourceURL}}\"></use>\n        </svg>\n        <!--<input type=\"text\" class=\"slds-input\" placeholder=\"Search\" ng-model=\"$parent.searchTerm\"/>-->\n        <input type=\"text\" class=\"slds-input\" placeholder=\"Search\" ng-model=\"importedScope.searchTerm\" ng-model-options=\"{ updateOn: 'default blur', debounce: { 'default': 1000, 'blur': 0 } }\" ng-change=\"importedScope.searchProducts()\" />\n    </div>\n    <div class=\"slds-col slds-no-flex cpq-product-filter\">\n        <section class=\"slds-clearfix slds-text-body--small\">\n            <div class=\"slds-float--left\">\n                <a>\n                    SORT BY NAME\n                    <!--<span class=\"slds-icon_container\">-->\n                    <!--    <svg aria-hidden=\"true\" class=\"slds-icon slds-icon--x-small slds-icon-text-default\">-->\n                    <!--        <use xlink:href=\"{{'/assets/icons/utility-sprite/svg/symbols.svg#chevrondown' | sldsStaticResourceURL}}\"></use>-->\n                    <!--    </svg>-->\n                    <!--</span>-->\n                </a>\n            </div>\n            <div class=\"slds-float--right\">\n                <a ng-click=\"showFilters = !showFilters\">\n                <!--<p ng-click=\"importedScope.openFilter()\">-->\n                    FILTER\n                    <!--<span class=\"slds-icon_container\">-->\n                    <!--    <svg aria-hidden=\"true\" class=\"slds-icon slds-icon--x-small slds-icon-text-default\">-->\n                    <!--        <use xlink:href=\"{{'/assets/icons/utility-sprite/svg/symbols.svg#chevrondown' | sldsStaticResourceURL}}\"></use>-->\n                    <!--    </svg>-->\n                    <!--</span>-->\n                </a>\n            </div>\n        </section>\n    </div>\n    <div class=\"slds-col cpq-items-container scroll cpq-product-filter\" ng-show=\"showFilters\">\n        <vloc-layout layout-name=\"cpq-product-filters\" ctrl=\"CPQProductListController\"></vloc-layout>\n    </div>\n    <div class=\"slds-col cpq-items-container scroll\" ng-show=\"!showFilters\">\n        <div class=\"slds-spinner_container\" ng-show=\"!isLoaded\">\n            <div class=\"slds-spinner--brand slds-spinner slds-spinner--medium\" aria-hidden=\"false\" role=\"alert\">\n                <div class=\"slds-spinner__dot-a\"></div>\n                <div class=\"slds-spinner__dot-b\"></div>\n            </div>\n        </div>\n        <vloc-card ng-repeat=\"card in cards |orderBy:order|filter: searchFunc(card)\" ctrl=\"CPQProductItemController\" ng-model=\"card\" data=\"card\" records=\"records\" index=\"{{$index}}\" session-id=\"{{sessionId}}\" ng-if=\"isLoaded\"></vloc-card>\n        <div class=\"slds-grid slds-grid--vertical-align-center slds-grid--align-center cpq-no-products-msg\" ng-if=\"records.length === 0\">\n            No products available.\n        </div>\n        <div class=\"next-product slds-col slds-text-align--center slds-m-vertical--medium\"  ng-if=\"session.nextProducts\">\n            <div class=\"slds-spinner_container\" ng-show=\"loadingMore\">\n                <div class=\"slds-spinner--brand slds-spinner slds-spinner--small\" aria-hidden=\"false\" role=\"alert\">\n                    <div class=\"slds-spinner__dot-a\"></div>\n                    <div class=\"slds-spinner__dot-b\"></div>\n                </div>\n            </div>\n            <a ng-click=\"importedScope.nextPage()\" ng-if=\"!loadingMore\">\n              Load More\n            </a>\n        </div>\n    </div>\n    <!-- Adding several products at one is not supported for now\n    <div class=\"slds-col slds-no-flex\">\n        <div class=\"slds-grid cpq-product-actions\">\n            <button class=\"slds-col slds-button slds-button--brand\"\n            ng-click=\"importedScope.addProducts()\"\n            ng-disabled=\"importedScope.productsToAdd.length === 0\">\n                Add to cart\n            </button>\n            <button class=\"slds-col slds-button slds-button--brand\"\n            ng-click=\"importedScope.compare()\"\n            ng-disabled=\"importedScope.productsToCompare.length < 2\">\n                Compare\n            </button>\n        </div>\n    </div>\n    -->\n</div>",
