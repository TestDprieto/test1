body{margin:10px;padding:0}.vlocity .vlc-overlay{position:fixed;z-index:1038;background-color:hsla(0,0%,100%,.9);width:100%;height:100%}.vlocity .vlc-overlay .spinner{position:fixed;top:50%;left:50%;margin-top:-14px;margin-left:-14px}.vlocity .modal-backdrop{opacity:.5}.vlocity .modal-header{padding:10px 15px 8px}.vlocity .modal-footer{padding:13px 15px 8px}.vlocity .attachment-list .attachments-hd{margin-bottom:10px;text-align:center}.vlocity .attachment-list table.tbl-attachments{margin-bottom:0}.vlocity .attachment-list table.tbl-attachments tr td{overflow:initial;text-overflow:clip;white-space:normal;word-break:break-all;width:auto}.vlocity .attachment-list table.tbl-attachments tr .action{width:130px}.vlocity .attachment-list table.tbl-attachments tr .name{width:200px}.vlocity .attachment-list table.tbl-attachments tr .content-type{width:110px}.vlocity .attachment-list table.tbl-attachments tr .default{text-align:center;width:110px}.vlocity .attachment-list table.tbl-attachments tr .default .glyphicon{font-size:17px}.vlocity .attachment-list table.tbl-attachments tr .sequence{width:80px}.vlocity .attachment-list table.tbl-attachments tr .channel{width:200px}