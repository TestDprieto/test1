                        "%vlocity_namespace%__Sass__c": "/*Variables*/\n@import \"vloc-mob-con-variables\";\n\n/*Swiper*/\n.product-swiper-container.swiper-container-horizontal.swiper-container-coverflow {\n    position: absolute;\n    top: 75px;\n    width: 100%;\n    height: 325px;\n    left: 0px;\n}\n.con-product-image.swiper-slide img {\n    height: 300px;\n    width: auto;\n    display: block;\n    margin: auto;\n}\nvloc-mobile .product-swiper-container .con-product-image.swiper-slide{\n    width:285px !important;\n    height: 325px !important;\n    min-height: 325px !important;\n}\n.product-swiper-container > .swiper-pagination.swiper-pagination-clickable.swiper-pagination-bullets {\n    top: 305px !important;\n    position: absolute;\n}\nvloc-mobile .product-swiper-container .swiper-pagination-bullet{\n    background:rgba(0,0,0,0.5) !important;\n}\nvloc-mobile .product-swiper-container .swiper-pagination-bullet-active{\n    background:rgba(0,0,0,1) !important;\n}\nvloc-mobile .product-swiper-container > .swiper-pagination-bullets .swiper-pagination-bullet {\n    margin: 0 4px !important;\n}\n\n/*Product Images*/\n.con-product{\n    width:100%;\n    height:100%;\n    background:#FFF;\n    padding-top:70px;\n    box-sizing:border-box;\n    opacity:0;\n    transition:opacity 1s linear;\n}\n.con-product-images{\n    height:325px;\n    width:100%;\n}\n.con-product-images-background{\n    width: 275px;\n    height: 275px;\n    margin:auto;\n    border-radius:300px;\n    background: $circle-colour-1;\n}\n\n/*Product Details*/\n.con-product-details{\n    margin-top: 25px;\n    padding:0px 25px 0px 25px;\n}\n.con-product-details-rating{\n    width: 100%;\n    height: 15px;\n    color: #6a6f7f;\n    margin-bottom: 10px;\n    font-size: 12px;\n    line-height: 15px;\n}\n.con-product-details-rating span{\n    margin-left:5px;\n}\n.con-product-details-rating img{\n    float: left;\n    margin-right: 1px;\n    height: 12px;\n    width: 13px;\n}\n.con-product-details-title{\n    font-size:28px;\n    font-family:Brown-Bold;\n    margin-bottom:25px;\n    line-height: 28px;\n}\n.con-product-details-price-large{\n    font-size:34px;\n    color:$theme-colour;\n    margin-bottom: 5px;\n    font-family: Brown-Bold;\n    height: 34px;\n    line-height: 34px;\n}\n.con-product-details-price-large span{\n    font-size:16px;\n}\n.con-product-details-price-small{\n    font-size:14px;\n    color:$subtitle-colour;\n}\n.con-product-details-price-small span{\n    color:$theme-colour;\n}\n\n/*Product Attributes*/\n/*Colours*/\n.con-product-attributes{\n    margin-top: 15px;\n    padding:0px 25px 0px 25px;\n}\n.con-product-attributes-colours-title{\n    font-size:14px;\n    color:$subtitle-colour;\n    margin-bottom: 10px;\n}\n.con-product-attributes-colours{\n    display:table;\n    width: 100%;\n    margin-bottom: 15px; \n}\n.con-product-attributes-colours-option{\n    width: 30px;\n    height: 30px;\n    border-radius: 30px;\n    float: left;\n    margin-right: 15px;\n    position:relative;\n    z-index:4;\n    margin-bottom: 15px;\n}\n.con-product-attributes-colours-selected{\n    border:4px solid #FFF;\n    box-sizing:border-box;\n}\n.con-product-attributes-colours-selected:after{\n    position:absolute;\n    top:-4px;\n    left:-4px;\n    border: 2px solid $theme-colour;\n    width:30px;\n    height:30px;\n    background:transparent;\n    content: ' ';\n    border-radius: 30px;\n    z-index:5;\n}\n/*Select Boxes*/\n.con-product-attributes-select-option{\n    width:calc(50% - 5px);\n    float:left;\n    //min-height:95px;\n    border:1px solid #e9ecfe;\n    box-sizing:border-box;\n    text-align:center;\n    padding:10px;\n    border-radius: 10px;\n    margin-bottom:10px;\n}\n.con-product-attributes-select-option-selected{\n    border: 2px solid $theme-colour;\n    margin-bottom: 8px;\n    padding:8px;\n}\n.con-product-attributes-select-option-title{\n    color:#000;\n    font-size:18px;\n    font-family:Brown-Bold;\n    margin-bottom:10px;\n    pointer-events:none;\n}\n.con-product-attributes-select-option-subtitle{\n    font-size:14px;\n    color:$subtitle-colour;\n    pointer-events:none;\n}\n.con-product-attributes-select {\n    display: table;\n    width: 100%;\n    margin-top:15px;\n}\n.con-product-attributes-select > div:nth-child(odd) > .con-product-attributes-select-option {\n    margin-right: 10px;\n}\n.con-product-attributes-select > div:nth-child(odd) > .con-product-attributes-select-option-selected{\n    margin-right: 8px;\n}\n\n\n/*Product Button*/\n.con-product-button{\n    color: #ffffff;\n    font-size: 14px;\n    background: $theme-colour;\n    border-radius: 5px;\n    line-height: 40px;\n    width: calc(100% - 50px);\n    height: 40px;\n    display: table;\n    padding: 0px 25px;\n    text-align:center;\n    margin:auto;\n    margin-top:35px;\n    margin-bottom:25px;\n}\n.con-product-button-added{\n    background: $discount-colour;\n}\n\n/*Product Description*/\n.con-product-description span{\n    margin-bottom:25px;\n    color:#000;\n}\n.con-product-description-more{\n    font-size:16px;\n    color:$theme-colour;\n    margin-top: 25px;\n    font-family: Brown-Bold;\n}\n\n/*Product Reviews*/\n.con-product-reviews-single .con-product-details-rating{\n    margin-bottom: 5px;\n    margin-top: 5px;\n}\n.con-product-reviews-single {\n    margin: 25px 0px;\n}\n.con-star-empty{\n    opacity:0.5;\n}\n.con-product-reviews-single span{\n    color:$subtitle-colour;\n}\n.con-product-reviews-overview-total{\n    text-align:center;\n    width:70px;\n    line-height:44px;\n    font-size: 44px;\n    font-family: Brown-Bold;\n    float:left;\n}\n.con-product-reviews-overview-total span{\n    display:block;\n    width:100%;\n    font-size:18px;\n    line-height: 22px;\n}\n.con-product-reviews-overview-total .con-product-details-rating{\n    margin-top:5px;\n    margin-bottom:5px;\n}\n.con-product-reviews-overview-bars{\n    float:right;\n    width: calc(100% - 70px);\n    box-sizing:border-box;\n    padding-left:25px;\n}\n.con-product-reviews-overview-bars-bar-background{\n    width: calc(100% - 60px);\n    height: 12px;\n    background: #fce4bd;\n    float: left;\n    border-radius:2px;\n}\n.con-product-reviews-overview-bars-bar-fill{\n    height:12px;\n    background:#f5a623;\n    border-radius:2px;\n}\n.con-product-reviews-overview-bars-bar-rating{\n    float: left;\n    width: 30px;\n    height: 12px;\n    line-height: 10px;\n    font-size: 12px;\n}\n.con-product-reviews-overview-bars-bar span{\n    width: 24px;\n    float: right;\n    font-size: 12px;\n    height: 12px;\n    line-height: 12px;\n    margin-left: 5px;\n}\n.con-product-reviews-overview-bars-bar {\n    display: table;\n    width: 100%;\n    margin-bottom:6px;\n}\n.con-product-reviews-overview-bars-bar-rating img{\n    height:10px;\n}\n.con-product-reviews-overview {\n    display: table;\n    width: 100%;\n}\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n@media only screen \n    and (max-device-height : 640px) {\n \n}\n/*iPhone 6/7/8*/\n@media only screen \n    and (max-device-height : 667px) {\n\n}\n/*iPhone 6/7/8 Plus*/\n@media only screen \n    and (min-device-height : 668px) and (max-device-height : 737px) {\n\n}\n/*iPhone X*/\n@media only screen \n    and (min-device-height : 738px) {\n\n}",
