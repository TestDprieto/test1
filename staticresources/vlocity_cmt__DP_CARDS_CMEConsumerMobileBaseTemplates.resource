                        "%vlocity_namespace%__HTML__c": "<!-- Donut Graphs -->\n<div ng-controller=\"donutChartController as dc\">\n    <div class=\"con-device-details-donut-graph-chart\" ng-if=\"placeholder.DisplayThreshold.value == '0'\">\n        <div class=\"con-device-details-donut-graph-title\">\n            {{placeholder.DisplayName.value}}\n        </div>\n        <div class=\"con-device-details-donut-graph-data con-icon-infinity\"></div>\n        <svg width=\"70\" height=\"70\" xmlns=\"http://www.w3.org/2000/svg\">\n            <g>\n                <circle id=\"circle\" class=\"circle_animation circle_empty\" r=\"30\" cy=\"35\" cx=\"35\" stroke-width=\"8\" stroke-dashoffset=\"0\" fill=\"none\"/>\n            </g>\n        </svg>\n        <div class=\"con-device-details-donut-graph-subtitle\">\n            {{placeholder.DisplayAmount.value}} {{placeholder.DisplayAmount.label}}\n        </div>\n    </div>\n    <div class=\"con-device-details-donut-graph-chart con-donut-chart-{{data.cardIndex}}\" ng-if=\"placeholder.DisplayThreshold.value != '0'\">\n        <div class=\"con-device-details-donut-graph-title\">\n            {{placeholder.DisplayName.value}}\n        </div>\n        <div class=\"con-device-details-donut-graph-data\"></div>\n        <svg width=\"70\" height=\"70\" xmlns=\"http://www.w3.org/2000/svg\">\n            <g>\n                <circle id=\"circle\" r=\"30\" cy=\"35\" cx=\"35\" stroke-width=\"8\" stroke=\"#e9ecfe\" fill=\"none\"/>\n                <circle id=\"circle\" class=\"circle_animation\" r=\"30\" cy=\"35\" cx=\"35\" stroke-width=\"8\" stroke-dashoffset=\"190\" fill=\"none\"/>\n            </g>\n        </svg>\n        <div class=\"con-device-details-donut-graph-subtitle\">\n            {{placeholder.DisplayAmount.value}}/{{placeholder.DisplayThreshold.value}} {{placeholder.DisplayAmount.label}}\n        </div>\n        {{drawGraph(data)}}\n    </div>\n</div>",
