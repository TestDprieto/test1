  margin: auto;\n  display: block;\n  transition: transform 0.5s linear; }\n\n/*Device Shadow*/\n.con-device-shadow {\n  width: 100%;\n  height: 50vh;\n  background: linear-gradient(to bottom, transparent 1%, rgba(0, 0, 0, 0.85) 50%, #000000 100%);\n  position: absolute;\n  top: 50vh;\n  left: 0px; }\n\n/*Device Chat */\n.con-device-chat {\n  width: 100%;\n  padding-top: 100px;\n  padding-bottom: 50px;\n  display: none; }\n\n.con-device-details-expanded .con-device-chat {\n  display: block; }\n\n.con-device-chat-icon {\n  width: 60px;\n  height: 60px;\n  border-radius: 60px;\n  border: 1.5px solid #0081f9;\n  box-shadow: 0px 0px 13px #0081f9;\n  margin: 0px auto 15px auto;\n  background: #FFFFFF;\n  position: relative; }\n\n.con-device-chat-icon-lines {\n  margin: 20px auto;\n  display: table; }\n\n.con-device-chat-icon-lines > div {\n  float: left;\n  width: 2px;\n  background: #0081f9;\n  border-radius: 10px; }\n\n.con-device-chat-icon-line1, .con-device-chat-icon-line5 {\n  height: 5px;\n  margin: 5px 1.5px;\n  opacity: 0.6;\n  animation: growlarge 4s 1s infinite; }\n\n.con-device-chat-icon-line5 {\n  animation-delay: 4s; }\n\n.con-device-chat-icon-line2, .con-device-chat-icon-line4 {\n  height: 9px;\n  margin: 3px 1.5px;\n  opacity: 0.7;\n  animation: growmedium 4s 3s infinite; }\n\n.con-device-chat-icon-line2 {\n  animation-delay: 5s; }\n\n.con-device-chat-icon-line3 {\n  height: 13px;\n  margin: 1px 1.5px;\n  opacity: 0.8;\n  animation: growsmall 4s 2s infinite; }\n\n.con-device-chat-icon-lines:after {\n  content: ' ';\n  width: 60px;\n  height: 60px;\n  border-radius: 60px;\n  background: #0081f9;\n  position: absolute;\n  top: -1.5px;\n  left: -1.5px;\n  z-index: -1;\n  animation: spin 6s linear infinite;\n  opacity: 0.5; }\n\n.con-device-chat-subtitle {\n  color: #0081f9;\n  text-align: center;\n  width: 100%; }\n\n.con-device-chat-title {\n  color: #0081f9;\n  font-family: Brown-Bold;\n  text-align: center;\n  width: 100%; }\n\n/*Chat Bot Icon Animations*/\n@keyframes growlarge {\n  0% {\n    transform: scaleY(1); }\n  15% {\n    transform: scaleY(1.75); }\n  30% {\n    transform: scaleY(0.6); }\n  45% {\n    transform: scaleY(1.5); }\n  60% {\n    transform: scaleY(0.7); }\n  75% {\n    transform: scaleY(2); }\n  90%, 100% {\n    transform: scaleY(1); } }\n\n@keyframes growmedium {\n  0% {\n    transform: scaleY(1); }\n  15% {\n    transform: scaleY(1.3); }\n  30% {\n    transform: scaleY(0.85); }\n  45% {\n    transform: scaleY(1.15); }\n  60% {\n    transform: scaleY(0.7); }\n  75% {\n    transform: scaleY(1.45); }\n  90%, 100% {\n    transform: scaleY(1); } }\n\n@keyframes growsmall {\n  0% {\n    transform: scaleY(1); }\n  15% {\n    transform: scaleY(1.25); }\n  30% {\n    transform: scaleY(0.9); }\n  45% {\n    transform: scaleY(1.1); }\n  60% {\n    transform: scaleY(0.85); }\n  75% {\n    transform: scaleY(1.4); }\n  90%, 100% {\n    transform: scaleY(1); } }\n\n@keyframes spin {\n  0% {\n    transform: rotate(0deg) scale(1, 1); }\n  5% {\n    transform: rotate(180deg) scale(1.06, 1.1); }\n  10% {\n    transform: rotate(360deg) scale(1.02, 1); }\n  15% {\n    transform: rotate(540deg) scale(1, 1); }\n  20% {\n    transform: rotate(720deg) scale(1, 1); }\n  25% {\n    transform: rotate(900deg) scale(1.09, 1.15); }\n  30% {\n    transform: rotate(1080deg) scale(1.02, 1); }\n  35% {\n    transform: rotate(1260deg) scale(1, 1); }\n  40% {\n    transform: rotate(1440deg) scale(1, 1); }\n  45% {\n    transform: rotate(1620deg) scale(1.12, 1.19); }\n  50% {\n    transform: rotate(1800deg) scale(1.02, 1); }\n  55% {\n    transform: rotate(1980deg) scale(1, 1); }\n  100% {\n    transform: rotate(0deg) scale(1, 1); } }\n\n/*Product Browse*/\n.con-shop-browse {\n  display: table;\n  min-height: 500px; }\n\n.con-shop-browse-product {\n  width: calc(50% - 12.5px);\n  display: table;\n  float: left;\n  margin-bottom: 25px; }\n\n.con-shop-browse-product:nth-child(odd) {\n  margin-right: 25px; }\n\n.con-shop-browse-product-background {\n  background: #dadbdf;\n  height: 150px;\n  width: 150px;\n  border-radius: 100%;\n  padding-top: 10px;\n  margin-bottom: 25px; }\n\n.con-shop-browse-product:nth-child(4n+1) .con-shop-browse-product-background, .con-shop-browse-product:nth-child(4n+4) .con-shop-browse-product-background {\n  background: #7fbffc; }\n\n.con-shop-browse-product-image img {\n  height: 160px;\n  margin: auto;\n  display: block;\n  width: auto; }\n\n.con-shop-browse-product-discount-percentage {\n  color: #1e9b62;\n  min-height: 20px;\n  min-width: 20px; }\n\n.con-shop-browse-product-name {\n  font-size: 16px;\n  color: #0081f9;\n  font-family: Brown-Bold;\n  min-height: 20px;\n  max-height: 40px;\n  overflow: hidden;\n  margin-bottom: 10px;\n  /* white-space: nowrap; */\n  /* text-overflow: ellipsis; */ }\n\n.con-shop-browse-product-price {\n  font-size: 16px;\n  width: 50%;\n  float: left;\n  margin-bottom: 10px;\n  font-family: Brown-Bold; }\n\n.con-shop-browse-product-discount-price {\n  text-decoration: line-through;\n  color: #6a6f7f;\n  float: right;\n  width: 50%;\n  height: 20px;\n  margin-bottom: 10px; }\n\n.con-shop-browse-product-attribute-more {\n  width: 50%;\n  float: right;\n  font-size: 12px;\n  font-family: Brown-Light;\n  line-height: 18px;\n  height: 18px; }\n\n.con-shop-browse-product-attribute-colour {\n  width: 18px;\n  height: 18px;\n  float: left;\n  border-radius: 20px;\n  margin-right: 5px; }\n\n.con-shop-browse-product-attribute-colour-border {\n  border: 1px solid #e9ecfe; }\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n@media only screen and (max-device-height: 640px) {\n  .con-device-details-donut-graph {\n    display: none; }\n  .con-device-details-wrapper {\n    transform: translateY(calc(100vh - 250px)); }\n  .con-device-title {\n    top: calc(100vh - 310px); } }\n\n/*iPhone 6/7/8*/\n@media only screen and (min-device-height: 641px) and (max-device-height: 667px) {\n  .con-device-details-bar-graph {\n    display: none; }\n  .con-device-details-wrapper {\n    transform: translateY(calc(100vh - 260px)); }\n  .con-device-title {\n    top: calc(100vh - 320px); } }\n\n/*iPhone 6/7/8 Plus*/\n@media only screen and (min-device-height: 668px) and (max-device-height: 737px) {\n  .con-device-details-donut-graph {\n    display: none; }\n  .con-device-details-wrapper {\n    transform: translateY(calc(100vh - 340px)); }\n  .con-device-title {\n    top: calc(100vh - 405px); } }\n\n/*iPhone X*/\n@media only screen and (min-device-height: 738px) {\n  .con-device-details-donut-graph {\n    display: none; }\n  .con-device-details-wrapper {\n    transform: translateY(calc(100vh - 340px)); }\n  .con-device-title {\n    top: calc(100vh - 405px); } }\n",
                        "%vlocity_namespace%__CustomJavascript__c": "vlocity.cardframework.registerModule.controller('assetPageController',\n                ['$scope', '$state', '$rootScope', '$ionicSideMenuDelegate',\n                function($scope, $state, $rootScope, $ionicSideMenuDelegate) {\n \n        $scope.vm = Object.assign($scope, $scope.parent);\n \n        $scope.vm.toggleSideMenu = function() {\n            $ionicSideMenuDelegate.toggleLeft();\n        }\n        \n        this.viewDetail = function(name) {\n                    $state.go('app.univ', {\n\t\t\t\t\ttype: name\n\t\t\t\t});\n              \n        };\n\n}]);",
