box-sizing: border-box;\n    text-align: center;\n    box-shadow: 0px 0px 30px rgba(0,0,0,0.4);\n    min-height: 200px;\n}\n.con-home-widget-banner-title{\n    color:#000000;\n    font-size:28px;\n    font-weight:bold;\n    width:100%;\n    line-height: 30px;\n    margin-bottom: 15px;\n}\n.con-home-widget-banner-subtitle{\n    color:$subtitle-colour;\n    font-size:16px;\n    width:100%;\n    margin-bottom: 15px;\n}\n.con-home-widget-banner-button{\n    color: #ffffff;\n    font-size: 14px;\n    background: $theme-colour;\n    border-radius: 25px;\n    line-height: 40px;\n    margin: auto;\n    width: auto;\n    height: 40px;\n    display: table;\n    padding: 0px 25px;\n}\n\n/*Home Page Actions*/\n.con-home-widget-action{\n    width: calc(100% - 50px);\n    margin: 25px auto;\n    color:$white-text-colour;\n    text-align:left;\n}\n\n.con-home-border{\n    border-bottom:1px solid rgba(255,255,255,0.3);\n    width: calc(100% - 50px);\n    height:0px;\n    margin:auto;\n}\n.con-home-widget-action-title{\n    font-size:28px;\n    font-weight:bold;\n    width:100%;\n    line-height: 30px;\n    margin-bottom: 15px; \n}\n.con-home-widget-action-subtitle{\n    font-size:16px;\n    width:100%;\n    margin-bottom: 25px;\n    font-family:Brown-Light;\n}\n.con-home-widget-action-arrow{\n    font-size: 24px;\n    float: right;\n    position: relative;\n    top: -70px;\n    font-family:fontello;\n}\n\n/*Home Page Grabber*/\n.con-home-grabber{\n    position: absolute;\n    bottom: 25px;\n    width: 168px;\n    height: 32px;\n    line-height: 32px;\n    background: #fff;\n    border-radius: 20px;\n    color: #000;\n    left: 50%;\n    margin-left: -84px;\n    z-index: 4;\n    box-shadow: 0px 0px 30px rgba(0,0,0, 0.4);\n    font-weight: bold;\n    animation-name: bounce;\n    animation-duration: 10s;\n    animation-delay: 5s;\n    animation-iteration-count: infinite;\n}\n.con-down-arrow {\n    color: $theme-colour;\n    font-size: 12px;\n    position: relative;\n    left: 0px;\n    font-weight: bold;\n    top: 0px;\n    font-family: fontello;\n    margin-right: 5px;\n}\n@keyframes bounce {\n    5% {transform:translate3d(0,-10px,0);}\n    10% {transform:translate3d(0,0,0);}\n    15% {transform:translate3d(0,-10px,0);}\n    20%, 100% {transform:translate3d(0,0,0);}\n}\n\n/*Home Page Titles*/\n.con-home-back{\n    color: #fff;\n    position: absolute;\n    left: 0;\n    top: 0;\n    z-index: 2;\n    font-size: 16px;\n    font-family: Brown-Light;\n    transition: opacity 0.5s linear;\n    width: 100px;\n    height: 70px;\n    padding-top: 45px;\n    padding-left: 25px;\n}\n.con-app-title{\n    color:#fff;\n    position:absolute;\n    left:50%;\n    margin-left:-50px;\n    width:100px;\n    top:45px;\n    font-size:20px;\n    text-align:center;\n    transition:opacity 0.5s linear;\n}\n.con-home-shop{\n    color: #fff;\n    position: absolute;\n    right: 0;\n    top: 0;\n    z-index: 2;\n    font-size: 22px;\n    font-family: fontello;\n    width: 100px;\n    height: 70px;\n    padding-top: 50px;\n    padding-right: 25px;\n    text-align: right;\n}\n.con-home-nav{\n    color: #fff;\n    position: absolute;\n    left: 0;\n    top: 0;\n    z-index: 2;\n    transition: opacity 0.5s linear;\n    width: 100px;\n    height: 70px;\n    padding-top: 45px;\n    padding-left: 25px;\n    text-align: left;\n    font-size:34px;\n}\n.con-home-title{\n    position: relative;\n    left: 50%;\n    margin-left: -62px;\n    width: 124px;\n}\n\n/*Home Page Device Images*/\n.con-home-device-images{\n    position: absolute;\n    top: calc(80vh - 50px);\n    width: 100%;\n    max-height: 300px;\n    height:300px;\n    padding: 25px;\n    padding-top: 50px;\n    box-sizing: border-box;\n    background: -webkit-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%);\n}\n.con-home-device-images img{\n    display: block;\n    width: 45%;\n    float: left;\n    transition: transform 0.5s;\n    margin-right: -17.5%;\n    position:relative;\n    transform: skew(0deg,0deg);\n}\n.con-home-device-images > div{\n    position:relative;\n}\n.con-home-device-images > div:nth-child(1){\n    z-index:3;\n}\n.con-home-device-images > div:nth-child(2){\n    z-index:2;\n}\n.con-home-device-images > div:nth-child(3){\n    z-index:1;\n}\n.con-home-device-images-small img{\n    transform:translate3d(90px,0,0) scale(2) skew(0deg,5deg);\n}\n\n// override default\n.fullscreen {\n    .has-header {\n        margin-top: 0 !important;\n        padding-top:0;\n    }\n}\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n@media only screen \n    and (max-device-height : 640px) {\n        .con-home-device-images{\n            top:80vh;\n        }\n        .con-home-widget-action-subtitle{\n            display:none;\n        }\n        .con-home-widget-action-arrow{\n            top:-35px;\n        }\n}\n/*iPhone 6/7/8*/\n@media only screen \n    and (max-device-height : 667px) {\n        .con-home-device-images{\n            top:80vh;\n        }\n}\n/*iPhone 6/7/8 Plus*/\n@media only screen \n    and (min-device-height : 668px) and (max-device-height : 737px) {\n        .con-home-device-images{\n            top: calc(80vh - 50px);\n        }\n}\n/*iPhone X*/\n@media only screen \n    and (min-device-height : 738px) {\n        .con-home-device-images{\n            top: calc(80vh - 75px);\n        }\n        .con-home-widget-banner{\n            margin: 25px auto 50px auto;\n        }\n}",
