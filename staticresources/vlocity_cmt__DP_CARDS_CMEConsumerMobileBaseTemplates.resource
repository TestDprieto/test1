                        "%vlocity_namespace%__HTML__c": "<script>\n\n        $(document).ready(function(){\n            \n\t        //Change active shop tab\n    \t    $('.con-shop-tab').on('click',function(event){\n    \t        \n    \t        //Remove current active tabs\n    \t        $('.con-shop-tab').removeClass('con-shop-tab-active');\n    \t        $('.con-shop-tab-content').removeClass('con-shop-tab-content-active');\n    \t        $(event.target).addClass('con-shop-tab-active');\n    \t        \n    \t        //Delay adding new tab to force scroll area to top\n    \t        setTimeout(function(){\n    \t            $('#con-products-' + event.target.id.substring(8)).addClass('con-shop-tab-content-active');\n    \t            //$('.scroll-content').scrollTop(0);\n    \t        },500);\n    \t        \n    \t        //Display banner image\n    \t        if(event.target.id.substring(8) != '1'){\n    \t            $('.con-shop-details').addClass('con-shop-details-expanded');\n    \t            $('.con-navbar-shop').addClass('con-navbar-shop-alt');\n    \t        }else{\n    \t            $('.con-shop-details').removeClass('con-shop-details-expanded');\n    \t            $('.con-navbar-shop').removeClass('con-navbar-shop-alt');\n    \t        }\n    \t        \n    \t\t});\n    \t\t\n\t    });\n</script>\n<div class=\"navbar-template con-navbar-shop\" ng-controller=\"shopController as sc\">\n\t<div class=\"con-shop-header\">\n        <button ng-click=\"vm.goBack()\" class=\"button button-clear header-item nav-left-button\" ng-disabled=\"vm.isSavingInProgress\" ng-style=\"{'color':$root.appTheme.NavBarItemColor}\">\n    \t\t<span ng-show=\"vm.isEditingDetailPage && vm.isDetailTabSelected == true\">\n    \t\t\tCancel\n    \t\t</span>\n    \t\t<span ng-show=\"!vm.isEditingDetailPage && vm.canGoBack\">\n    \t\t\t<i class=\"icon ion-ios-arrow-back\"></i>\n    \t\t</span>\n    \t\t<span ng-show=\"!vm.canGoBack\">\n    \t\t\t<i class=\"icon ion-navicon\"></i>\n    \t\t</span>\n    \t</button>\n    \t\n    \t<div class=\"title title-left header-item\">\n    \t\t{{ pageTitle }}\n    \t</div>\n    \t\n    \t<div class=\"con-home-shop\" ng-click=\"sc.viewDetail('')\">\n            \n        </div>\n\t</div>\n\n    <!-- Custom Content-->\n    <!-- Shop Banner -->\n    <div class=\"con-shop-banner\">\n        <img src=\"{{$root.customDefinedValue.CommunityURL + '/resource/VLOCMOBCON/images/img_PixelAd@2x.png' | trusted}}\"/>\n        <div class=\"con-shop-banner-title\">\n            Google Pixel 2 XL sale\n            <span></span>\n        </div>\n    </div>\n    \n    <!-- Shop Tabs-->\n    <div class=\"con-shop-tabs\">\n        <div class=\"con-shop-tab con-shop-tab-active\" id=\"con-tab-1\">\n            For You\n        </div>\n        <div class=\"con-shop-tab\" id=\"con-tab-2\">\n            Phones\n        </div>\n        <div class=\"con-shop-tab\" id=\"con-tab-3\">\n            Equipment\n        </div>\n        <div class=\"con-shop-tab\" id=\"con-tab-4\">\n            Accessories\n        </div>\n    </div>\n\n</div>",
