                        "%vlocity_namespace%__CSS__c": "/*Variables*/\n/* Variables */\n/*#0081f9;*/\n/*#6a6f7f;*/\n/*#FFFFFF*/\n/*#1e9b62;*/\n/*#7fbffc;*/\n/*#dadbdf;*/\n.menu {\n  width: 100vw !important; }\n\n.con-menu-profile {\n  width: 100%;\n  padding-top: 55px;\n  padding-left: 25px; }\n\n.con-menu-close {\n  position: absolute;\n  top: 20px;\n  left: 0px;\n  font-family: fontello;\n  font-size: 20px;\n  pointer-events: all;\n  padding: 25px; }\n\n.con-menu-settings {\n  float: right;\n  font-family: fontello;\n  font-size: 20px;\n  position: relative;\n  right: 25px;\n  top: 15px; }\n\n.con-menu {\n  background: #0081f9 !important;\n  color: #FFFFFF !important;\n  font-family: Brown-Regular;\n  font-size: 18px;\n  position: initial;\n  display: table;\n  width: 100vw;\n  height: 100%;\n  margin-top: 0 !important; }\n\nion-side-menu > ion-header-bar.bar.menu-bar.con-menu-header {\n  min-height: 150px !important;\n  background-image: url(\"https://vdo-cmt-site.secure.force.com/resource/VLOCMOBCON/images/bgtransparecny_Vlocity.png\") !important;\n  background-color: #0081f9 !important;\n  color: #FFFFFF !important;\n  background-size: 100% auto !important;\n  box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.5);\n  margin: 0px;\n  padding: 0px;\n  position: initial !important; }\n\n.list {\n  padding-top: 12.5px; }\n\n.con-menu .item {\n  border: 0px;\n  background: none;\n  color: #FFFFFF;\n  font-size: 20px;\n  font-family: Brown-Light;\n  font-weight: 300 !important;\n  padding: 12.5px 25px 12.5px 25px;\n  height: 50px; }\n\n.con-menu-profile-picture {\n  float: left; }\n\n.vlocity .menu .con-menu-avatar {\n  margin: 0px;\n  width: 50px;\n  height: 50px;\n  border: 2px solid #FFF;\n  border-radius: 30px; }\n\n.con-menu-name {\n  color: #FFF !important;\n  font-family: Brown-Regular;\n  margin-left: 25px;\n  float: left;\n  font-size: 24px;\n  margin-top: 15px; }\n\n.con-menu-name span {\n  display: block;\n  font-size: 16px;\n  margin-top: 5px;\n  font-family: Brown-Light; }\n\n.menu-open ion-side-menu-content.menu-content.pane.menu-animated {\n  transform: translate3d(100vw, 0px, 0px) !important; }\n\n.con-menu-icon {\n  width: 25px;\n  font-family: fontello;\n  font-size: 20px;\n  display: block;\n  float: left;\n  text-align: center; }\n\n.vlocity .menu .menu-item-name {\n  margin-left: 25px !important; }\n",
