                        "%vlocity_namespace%__Sass__c": "/*Variables*/\n@import \"vloc-mob-con-variables\";\n\n\n/*Bill Details*/\n.con-bill{\n    width:100%;\n    height:100%;\n    background:#FFF;\n    padding:  110px 20px 20px 20px;\n    box-sizing:border-box;\n    opacity:0;\n    transition:opacity 1s linear;\n}\n.con-bill .con-device-details-section {\n    padding: 0;\n}\n.con-bill .con-device-details-section.graph {\n    margin-top: 30px;\n}\n.con-bill-details-title {\n    font-size: 24px;\n    color: #000;\n    font-weight: bold;\n    min-height: 28px;\n}\n.con-bill-details-total{\n    font-size:48px;\n    color:$theme-colour;\n    font-family: Brown-Bold;\n    line-height: 48px;\n    margin-bottom: 15px;\n}\n.con-bill-details-subtotal{\n    color:$subtitle-colour;\n    line-height:20px;\n}\n.con-bill-details-subtotal img{\n    width: 20px;\n    height: 20px;\n    position: relative;\n    top: 5px;\n    margin-right: 5px;\n}\n.con-bill-details-manage{\n    color:$theme-colour;\n    font-family: Brown-Bold;\n    margin-top:5px;\n}\n.con-bill-details-manage span{\n    font-family:fontello;\n    font-size: 12px;\n    margin-left: 5px;\n}\n.con-bill-details-section{\n    padding:25px;\n    text-align:left;\n}\n\n/*Bill Table*/\n.con-bill-details-table-header{\n    font-family:Brown-Bold;\n    width:100%;\n    border-bottom:1px solid #000;\n    height:50px;\n    line-height:50px;\n    font-size:16px;\n}\n.con-bill-details-table-row{\n    width:100%;\n    border-bottom:1px solid #e9ecfe;\n    height:50px;\n    line-height:50px;\n}\n.con-bill-details-table-label{\n    width:70%;\n    color:$subtitle-colour;\n    float:left;\n}\n.con-bill-details-table-value{\n    width:30%;\n    float:right;\n    text-align:right;\n    height: 50px;\n    font-family: Brown-Bold;\n}\n.con-bill-details-table-value span{\n    font-family:fontello;\n    color:$theme-colour;\n    margin-left:10px;\n}\n.con-bill-details-table-row:last-child{\n    border-bottom:0px;\n    margin-bottom:25px;\n}\n\n/*Device Notifications*/\n.con-device-details-notification {\n  width: 100vw;\n  height: 90px;\n  box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.15);\n  margin-top: 25px;\n  position: relative;\n  margin-left: -20px;\n  padding: 0px 25px;\n  box-sizing: border-box; }\n\n.con-device-details-notification-icon {\n  width: 35px;\n  height: 35px;\n  float: left;\n  border-radius: 40px;\n  border: 2px solid #0081f9;\n  box-shadow: 0px 0px 13px #0081f9;\n  margin: 16px 15px 25px 0px; }\n\n.con-device-details-notification-title {\n  margin-bottom: 5px;\n  margin-left: 50px;\n  padding-top: 11px; }\n\n.con-device-details-notification-action {\n  color: #0081f9;\n  font-family: Brown-Bold; }\n\n.con-device-details-notification-action span {\n  font-family: fontello;\n  font-size: 12px;\n  margin-left: 5px; }\n\n.con-device-details-notification-icon-lines {\n  margin: 12px auto;\n  display: table; }\n\n.con-device-details-notification-icon-lines > div {\n  float: left;\n  width: 1px;\n  background: #0081f9;\n  border-radius: 10px; }\n\n.con-device-details-notification-icon-line1, .con-device-details-notification-icon-line5 {\n  height: 3px;\n  margin: 3px 1px;\n  opacity: 0.6; }\n\n.swiper-slide-active .con-device-details-notification-icon-line1, .swiper-slide-active .con-device-details-notification-icon-line5 {\n  animation: growlarge 4s 1s infinite; }\n  .con-device-details-notification-icon-line3 {\n  height: 7px;\n  margin: 1px 1px;\n  opacity: 0.8; }\n  .con-device-details-notification-icon-line2, .con-device-details-notification-icon-line4 {\n  height: 5px;\n  margin: 2px 1px;\n  opacity: 0.7; }\n  \n  /*Chat Bot Icon Animations*/\n@keyframes growlarge {\n  0% {\n    transform: scaleY(1); }\n  15% {\n    transform: scaleY(1.75); }\n  30% {\n    transform: scaleY(0.6); }\n  45% {\n    transform: scaleY(1.5); }\n  60% {\n    transform: scaleY(0.7); }\n  75% {\n    transform: scaleY(2); }\n  90%, 100% {\n    transform: scaleY(1); } }\n\n@keyframes growmedium {\n  0% {\n    transform: scaleY(1); }\n  15% {\n    transform: scaleY(1.3); }\n  30% {\n    transform: scaleY(0.85); }\n  45% {\n    transform: scaleY(1.15); }\n  60% {\n    transform: scaleY(0.7); }\n  75% {\n    transform: scaleY(1.45); }\n  90%, 100% {\n    transform: scaleY(1); } }\n\n@keyframes growsmall {\n  0% {\n    transform: scaleY(1); }\n  15% {\n    transform: scaleY(1.25); }\n  30% {\n    transform: scaleY(0.9); }\n  45% {\n    transform: scaleY(1.1); }\n  60% {\n    transform: scaleY(0.85); }\n  75% {\n    transform: scaleY(1.4); }\n  90%, 100% {\n    transform: scaleY(1); } }\n\n@keyframes spin {\n  0% {\n    transform: rotate(0deg) scale(1, 1); }\n  5% {\n    transform: rotate(180deg) scale(1.06, 1.1); }\n  10% {\n    transform: rotate(360deg) scale(1.02, 1); }\n  15% {\n    transform: rotate(540deg) scale(1, 1); }\n  20% {\n    transform: rotate(720deg) scale(1, 1); }\n  25% {\n    transform: rotate(900deg) scale(1.09, 1.15); }\n  30% {\n    transform: rotate(1080deg) scale(1.02, 1); }\n  35% {\n    transform: rotate(1260deg) scale(1, 1); }\n  40% {\n    transform: rotate(1440deg) scale(1, 1); }\n  45% {\n    transform: rotate(1620deg) scale(1.12, 1.19); }\n  50% {\n    transform: rotate(1800deg) scale(1.02, 1); }\n  55% {\n    transform: rotate(1980deg) scale(1, 1); }\n  100% {\n    transform: rotate(0deg) scale(1, 1); } }\n\n/*Bill Graph*/\n.con-bill-graph{\n    height:420px;\n    width:100%;\n    padding-top:70px;\n    pointer-events:none;\n    -webkit-backface-visibility: hidden;\n    -webkit-transform:translate3d(0,0,0);\n}\n.con-bill-graph-bar-wrapper{\n    width:calc(100% / 6);\n    height:234px;\n    float:left;\n    position:relative;\n    -webkit-backface-visibility: hidden;\n    -webkit-transform:translate3d(0,0,0);\n}\n.con-bill-graph-bar{\n    width:12px;\n    position:absolute;\n    bottom:30px;\n    left: 50%;\n    margin-left: -7.5px;\n    display: flex;\n    flex-direction: column-reverse;\n    -webkit-backface-visibility: hidden;\n    -webkit-transform:translate3d(0,0,0);\n}\n.con-bill-graph-bar-segment{\n    width:12px;\n    background:$theme-colour;\n    margin-top:2px;\n    transition:height 0.5s linear;\n    -webkit-backface-visibility: hidden;\n    -webkit-transform:translate3d(0,0,0);\n}\n.con-bill-graph-small .con-bill-graph-bar-segment{\n    height:0px !important;\n}\n.con-bill-graph-bar-axis{\n    width: 100%;\n    padding-top: 5px;\n    text-align: center;\n    height: 30px;\n    position: absolute;\n    bottom: 0px;\n    border-top: 1px solid #e9ecfe;\n    font-weight: bold;\n    -webkit-backface-visibility: hidden;\n    -webkit-transform:translate3d(0,0,0);\n}\n.con-bill-graph-legend{\n    width: 100%;\n    height: 20px;\n    font-family: Brown-Bold;\n    margin-bottom: 5px;\n    float: left;\n    margin-top: 10px;\n}\n.con-bill-graph-legend-bar{\n    width: 25px;\n    height: 10px;\n    float: left;\n    margin-right: 15px;\n    background: $theme-colour;\n    margin-top: 6px;\n    margin-left: 15px;\n}\n\n/*Bill Button*/\n.con-bill-button{\n    color: #ffffff;\n    font-size: 14px;\n    background: $theme-colour;\n    border-radius: 5px;\n    line-height: 40px;\n    width: 100%;\n    height: 40px;\n    display: table;\n    padding: 0px 25px;\n    text-align:center;\n    margin:auto;\n    margin-top:35px;\n}\n\n\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n@media only screen \n    and (max-device-height : 640px) {\n \n}\n/*iPhone 6/7/8*/\n@media only screen \n    and (max-device-height : 667px) {\n\n}\n/*iPhone 6/7/8 Plus*/\n@media only screen \n    and (min-device-height : 668px) and (max-device-height : 737px) {\n\n}\n/*iPhone X*/\n@media only screen \n    and (min-device-height : 738px) {\n}",
