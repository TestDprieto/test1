                        "%vlocity_namespace%__CSS__c": "/*Variables*/\n/* Variables */\n/*#0081f9;*/\n/*#6a6f7f;*/\n/*#FFFFFF*/\n/*#1e9b62;*/\n/*#7fbffc;*/\n/*#dadbdf;*/\n/*Usage Details*/\n.con-usage {\n  width: 100%;\n  height: 100%;\n  background: #FFF;\n  padding-top: 140px;\n  box-sizing: border-box;\n  transition: opacity 1s linear; }\n\n.con-usage-tab-content {\n  padding: 0px 15px; }\n\n.con-usage-details-title {\n  font-size: 24px;\n  color: #000;\n  font-weight: bold;\n  min-height: 28px; }\n\n.con-usage-details-total {\n  font-size: 48px;\n  color: #0081f9;\n  font-family: Brown-Bold;\n  line-height: 48px;\n  margin-bottom: 15px; }\n\n.con-usage-details-subtotal {\n  color: #6a6f7f;\n  line-height: 20px; }\n\n.con-usage-details-subtotal img {\n  width: 20px;\n  height: 20px;\n  position: relative;\n  top: 5px;\n  margin-right: 5px; }\n\n.con-usage-details-manage {\n  color: #0081f9;\n  font-family: Brown-Bold;\n  margin-top: 5px; }\n\n.con-usage-details-manage span {\n  font-family: fontello;\n  font-size: 12px;\n  margin-left: 5px; }\n\n.con-usage-details-section {\n  padding: 25px 0px;\n  text-align: left;\n  position: relative; }\n\n/*Usage Donut Table*/\n.con-usage-donut-wrapper {\n  min-height: 550px; }\n\n.con-usage-details-table-header {\n  font-family: Brown-Bold;\n  width: 100%;\n  border-bottom: 1px solid #000;\n  height: 50px;\n  line-height: 50px;\n  font-size: 16px; }\n\n.con-usage-details-table-row {\n  width: 100%;\n  border-bottom: 1px solid #e9ecfe;\n  height: 50px;\n  line-height: 50px; }\n\n.con-usage-details-table-label {\n  width: 60%;\n  color: #6a6f7f;\n  float: left;\n  pointer-events: none; }\n\n.con-usage-details-table-value {\n  width: 40%;\n  float: right;\n  text-align: right;\n  height: 50px;\n  font-family: Brown-Bold;\n  pointer-events: none; }\n\n.con-usage-details-table-value span {\n  font-family: fontello;\n  color: #0081f9;\n  margin-left: 10px;\n  pointer-events: none; }\n\n.con-usage-details-table-value .slds-icon-text-default {\n  fill: #0081f9; }\n\n.con-usage-details-table-row:last-child {\n  border-bottom: 0px;\n  margin-bottom: 20px; }\n\n.con-usage-dot {\n  width: 10px;\n  height: 10px;\n  border-radius: 12px;\n  margin-right: 25px;\n  display: block;\n  float: left;\n  position: relative;\n  top: 22px; }\n\n.circle_usage {\n  display: block;\n  margin: auto;\n  margin-top: 10px;\n  margin-bottom: 10px; }\n\n.circle_usage_animation {\n  transition: stroke-dasharray .5s linear, stroke-dashoffset .5s linear, stroke-width .5s linear; }\n\n.circle_usage_animation_active {\n  stroke-dashoffset: 503 !important;\n  stroke-dasharray: 503px !important; }\n\n.circle_usage_animation-selected {\n  stroke-width: 20px !important; }\n\n.con-usage-circle-title {\n  position: absolute;\n  top: 125px;\n  font-size: 34px;\n  font-family: Brown-Bold;\n  width: calc(100vw - 30px);\n  text-align: center; }\n\n.con-usage-circle-title span {\n  font-size: 16px; }\n\n.con-usage-circle-title span.con-usage-circle-subtitle {\n  font-size: 12px;\n  color: #6a6f7f;\n  display: block;\n  font-family: Brown-Light; }\n\n.con-usage-details-seciton {\n  position: relative; }\n\n/*Usage Notification*/\n.con-device-details-notification {\n  width: 100vw;\n  height: 90px;\n  box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.15);\n  margin-top: 25px;\n  position: relative;\n  margin-left: -25px;\n  padding: 0px 25px;\n  box-sizing: border-box; }\n\n.con-device-details-notification-icon {\n  width: 35px;\n  height: 35px;\n  float: left;\n  border-radius: 40px;\n  border: 1.5px solid #0081f9;\n  box-shadow: 0px 0px 13px #0081f9;\n  margin: 16px 15px 25px 0px; }\n\n.con-device-details-notification-title {\n  margin-bottom: 5px;\n  margin-left: 50px;\n  padding-top: 11px; }\n\n.con-device-details-notification-action {\n  color: #0081f9;\n  font-family: Brown-Bold; }\n\n.con-device-details-notification-action span {\n  font-family: fontello;\n  font-size: 12px;\n  margin-left: 5px; }\n\n.con-device-details-notification-icon-lines {\n  margin: 12px auto;\n  display: table; }\n\n.con-device-details-notification-icon-lines > div {\n  float: left;\n  width: 1px;\n  background: #0081f9;\n  border-radius: 10px; }\n\n.con-device-details-notification-icon-line1, .con-device-details-notification-icon-line5 {\n  height: 3px;\n  margin: 2.5px 1px;\n  opacity: 0.6; }\n\n.swiper-slide-active .con-device-details-notification-icon-line1, .swiper-slide-active .con-device-details-notification-icon-line5 {\n  animation: growlarge 4s 1s infinite; }\n\n.con-device-details-notification-icon-line3 {\n  height: 7px;\n  margin: 0.5px 1px;\n  opacity: 0.8; }\n\n.con-device-details-notification-icon-line2, .con-device-details-notification-icon-line4 {\n  height: 5px;\n  margin: 1.5px 1px;\n  opacity: 0.7; }\n\n/*Usage Graph*/\n.con-usage-graph {\n  height: 370px;\n  width: 100%;\n  padding-top: 50px;\n  pointer-events: none;\n  -webkit-backface-visibility: hidden;\n  -webkit-transform: translate3d(0, 0, 0);\n  position: relative; }\n\n.con-usage-graph-bar-wrapper {\n  width: calc(100% / 6);\n  height: 230px;\n  float: left;\n  position: relative;\n  -webkit-backface-visibility: hidden;\n  -webkit-transform: translate3d(0, 0, 0); }\n\n.con-usage-graph-bar {\n  width: 12px;\n  position: absolute;\n  bottom: 30px;\n  left: 50%;\n  margin-left: -7.5px;\n  -webkit-backface-visibility: hidden;\n  -webkit-transform: translate3d(0, 0, 0); }\n\n.con-usage-graph-bar-segment {\n  width: 12px;\n  background: #e9ecfe;\n  margin-top: 2px;\n  transition: height 0.5s linear;\n  -webkit-backface-visibility: hidden;\n  -webkit-transform: translate3d(0, 0, 0); }\n\n.con-usage-graph-small .con-usage-graph-bar-segment {\n  height: 0px !important; }\n\n.con-usage-graph-bar-segment-overage {\n  background: #ff2953; }\n\n.con-usage-graph-bar-axis {\n  width: 100%;\n  padding-top: 5px;\n  text-align: center;\n  height: 30px;\n  position: absolute;\n  bottom: 0px;\n  border-top: 1px solid #e9ecfe;\n  font-weight: bold;\n  -webkit-backface-visibility: hidden;\n  -webkit-transform: translate3d(0, 0, 0); }\n\n.con-usage-graph-legend {\n  width: 100%;\n  height: 20px;\n  font-family: Brown-Bold;\n  margin-bottom: 5px;\n  float: left;\n  margin-top: 25px; }\n\n.con-usage-graph-legend-bar {\n  width: 25px;\n  height: 10px;\n  float: left;\n  margin-right: 15px;\n  background: #ff2953;\n  margin-top: 6px;\n  margin-left: 15px; }\n\n.con-usage-graph-bar-overage {\n  width: 100%;\n  border-bottom: 1px dashed #6a6f7f;\n  position: absolute;\n  top: 50px;\n  left: 0px;\n  color: #6a6f7f;\n  line-height: 20px;\n  font-family: Brown-Light;\n  padding-bottom: 4px;\n  padding-left: 10px;\n  -webkit-backface-visibility: hidden;\n  -webkit-transform: translate3d(0, 0, 0);\n  font-size: 12px;\n  z-index: 1; }\n\n.con-usage-tab-content .con-device-details-section {\n  padding: 10px 0px 0px 0px; }\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n/*iPhone 6/7/8*/\n/*iPhone 6/7/8 Plus*/\n/*iPhone X*/\n",
                        "%vlocity_namespace%__CustomJavascript__c": "vlocity.cardframework.registerModule.controller('usageController',\n    ['$scope', '$rootScope', function($scope, $rootScope) {\n        \n        var topData = 1;\n        var topTalk = 1;\n        var topText = 1;\n        var thresholdData = 0;\n        var thresholdTalk = 0;\n        var thresholdText = 0;\n        var vm = this;\n        this.selectedTab = 1;\n        this.nsPrefix = $scope.nsPrefix;\n        var nsPrefix = $scope.nsPrefix;\n        \n        this.init = function(payload) {\n            for(var i = 0; i < payload.length; i++){\n\n                //Find highest usage or theshold\n                if(payload[i].expr0 > topData && payload[i][nsPrefix+'Type__c'] == 'Data'){\n                    topData = payload[i].expr0;\n                }\n                if(payload[i].expr1 > topData && payload[i][nsPrefix+'Type__c'] == 'Data'){\n                    topData = payload[i].expr1;\n                }\n                if(payload[i].expr0 > topTalk && payload[i][nsPrefix+'Type__c'] == 'Talk'){\n                    topTalk = payload[i].expr0;\n                }\n                if(payload[i].expr1 > topTalk && payload[i][nsPrefix+'Type__c'] == 'Talk'){\n                    topTalk = payload[i].expr1;\n                }\n                if(payload[i].expr0 > topText && payload[i][nsPrefix+'Type__c'] == 'Text'){\n                    topText = payload[i].expr0;\n                }\n                if(payload[i].expr1 > topText && payload[i][nsPrefix+'Type__c'] == 'Text'){\n                    topText = payload[i].expr1;\n                }\n                \n                //Find threshold\n                if(payload[i].expr1 > thresholdData && payload[i][nsPrefix+'Type__c'] == 'Data'){\n                    thresholdData = payload[i].expr1;\n                    console.log('thresholdData ', thresholdData);\n                }\n                if(payload[i].expr1 > thresholdTalk && payload[i][nsPrefix+'Type__c'] == 'Talk'){\n                    thresholdTalk = payload[i].expr1;\n                }\n                if(payload[i].expr1 > thresholdText && payload[i][nsPrefix+'Type__c'] == 'Text'){\n                    thresholdText = payload[i].expr1;\n                }\n            }\n            \n            $scope.topData = topData;\n            $scope.topTalk = topTalk;\n            $scope.topText = topText;\n            $scope.thresholdData = thresholdData;\n            $scope.thresholdTalk = thresholdTalk;\n            $scope.thresholdText = thresholdText;\n            \n            function debounce(func, wait, immediate) {\n                var timeout;\n                return function() {\n                    var context = this, args = arguments;\n                    var later = function() {\n                        timeout = null;\n                        if (!immediate) func.apply(context, args);\n                    };\n                    var callNow = immediate && !timeout;\n                    clearTimeout(timeout);\n                    timeout = setTimeout(later, wait);\n                    if (callNow) func.apply(context, args);\n                };\n            }\n            \n            var showGraphFn = debounce(function() {\n                var elements = document.querySelector('.con-usage-graph');\n                if (elements && elements.getBoundingClientRect()) {\n                    if (elements.getBoundingClientRect().top < (window.innerHeight / 3) * 2) {\n                        elements.classList.remove('con-usage-graph-small');\n                    } else {\n                        elements.classList.add('con-usage-graph-small');\n                    }\n                }\n            });\n            \n            var scrollContentContainers = document.querySelectorAll(\".scroll-content\");\n            for (var j = 0; j < scrollContentContainers.length; j++) {\n                scrollContentContainers[j].addEventListener(\"scroll\", showGraphFn);\n            }\n        }\n            \n        \n        $rootScope.$on('tabSelected', function(event, index) {\n            vm.selectedTab = index;\n        })\n\n}]);",
