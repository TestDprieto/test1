                        "%vlocity_namespace%__CSS__c": "/*Variables*/\n/* Variables */\n/*#0081f9;*/\n/*#6a6f7f;*/\n/*#FFFFFF*/\n/*#1e9b62;*/\n/*#7fbffc;*/\n/*#dadbdf;*/\n/*Usage Details*/\n.con-chat {\n  width: 100vw;\n  height: 100%;\n  background: #FFF;\n  box-sizing: border-box;\n  opacity: 0;\n  transition: opacity 1s linear;\n  overflow: hidden; }\n\n.con-chat-details-title {\n  font-size: 16px;\n  color: #000;\n  font-family: Brown-Bold;\n  min-height: 28px;\n  line-height: 50px; }\n\n/*Chat Notification*/\n.con-chat .con-device-chat {\n  display: block;\n  padding: 0px; }\n\n.con-chat .con-device-chat-icon {\n  float: left;\n  margin-right: 15px;\n  width: 50px;\n  height: 50px;\n  border-radius: 50px; }\n\n.con-chat .con-device-chat-icon-lines:after {\n  width: 50px;\n  height: 50px;\n  border-radius: 50px; }\n\n.con-chat .con-device-chat-icon-lines {\n  margin: 15px auto; }\n\n.con-chat .con-device-chat-icon-line1, .con-chat .con-device-chat-icon-line5 {\n  animation: growlarge 4s 1s infinite; }\n\n.con-chat .on-device-chat-icon-line5 {\n  animation-delay: 4s; }\n\n.con-chat .con-device-chat-icon-line2, .con-chat .con-device-chat-icon-line4 {\n  animation: growmedium 4s 3s infinite; }\n\n.con-chat .con-device-chat-icon-line2 {\n  animation-delay: 5s; }\n\n.con-chat .con-device-chat-icon-line3 {\n  animation: growsmall 4s 2s infinite; }\n\n/*Ionic keyboard fixes*/\n.keyboard-up {\n  padding-bottom: 44px !important;\n  overflow: hidden !important; }\n\n/*Chat Window*/\n.con-chat-window {\n  height: calc(100% - 235px);\n  position: absolute;\n  top: 60px;\n  left: 0px;\n  width: 100%;\n  padding: 25px;\n  box-sizing: border-box;\n  overflow: scroll; }\n\n.con-chat-text-left {\n  width: 100%;\n  padding-right: 20%;\n  float: left;\n  margin-bottom: 10px; }\n\n.con-chat-text-right {\n  width: 100%;\n  padding-left: 20%;\n  box-sizing: border-box;\n  float: right;\n  margin-bottom: 25px;\n  margin-top: 15px;\n  position: relative;\n  transition: opacity 0.5s linear; }\n\n.con-chat-text-right-active {\n  opacity: 0; }\n\n.con-chat-text-right span {\n  width: auto;\n  float: right;\n  background: #0081f9;\n  color: #fff;\n  border-radius: 10px;\n  padding: 20px;\n  border-bottom-right-radius: 0px;\n  position: relative;\n  display: block; }\n\n.con-chat-text-right span:after {\n  content: ' ';\n  width: 10px;\n  height: 10px;\n  border-left: 10px solid transparent;\n  border-right: 10px solid transparent;\n  border-bottom: 10px solid #0081f9;\n  position: absolute;\n  right: -5px;\n  bottom: 0px; }\n\n.con-chat-button {\n  color: #0081f9;\n  width: 100%;\n  height: 40px;\n  text-align: center;\n  line-height: 38px;\n  border: 1px solid #D9D9D9;\n  margin-bottom: 10px;\n  border-radius: 5px;\n  box-sizing: border-box;\n  font-family: Brown-Bold;\n  float: left; }\n\n.con-chat-buttons {\n  margin-top: 10px;\n  display: table;\n  float: left; }\n\n.con-chat-loading {\n  float: left;\n  width: 100%;\n  height: 20px; }\n\n.con-chat-loading span {\n  height: 8px;\n  width: 8px;\n  background: #D9D9D9;\n  margin-right: 8px;\n  border-radius: 8px;\n  display: block;\n  float: left;\n  animation: loading 1s infinite;\n  margin-top: 10px; }\n\n.con-chat-loading span:nth-child(n+2) {\n  animation: loading 1s 0.2s infinite; }\n\n.con-chat-loading span:nth-child(n+3) {\n  animation: loading 1s 0.4s infinite; }\n\n@keyframes loading {\n  0% {\n    transform: translate3d(0, 0, 0); }\n  33% {\n    transform: translate3d(0, -5px, 0); }\n  66% {\n    transform: translate3d(0, 0, 0); }\n  100% {\n    transform: translate3d(0, 0, 0); } }\n\n/*Chat Image Responses*/\n.con-chat-image {\n  border-radius: 10px;\n  overflow: hidden;\n  border: 1px solid #d4d4d4;\n  min-height: 146px; }\n\n.con-chat-image-title {\n  height: 40px;\n  padding: 8px 15px;\n  color: #0081f9; }\n\n.con-chat-image-title span {\n  float: right;\n  color: #6a6f7f; }\n\n/*Chat Input*/\n.con-chat-input {\n  position: absolute;\n  bottom: 0px;\n  left: 0px;\n  height: 160px;\n  line-height: 60px;\n  box-sizing: border-box;\n  width: 100vw; }\n\n.con-chat-input input[type=text] {\n  width: 100%;\n  padding: 0px 25px;\n  height: 60px;\n  color: #D9D9D9;\n  font-family: Brown-Light;\n  -webkit-appearance: none;\n  border-radius: 0px;\n  background: #fff;\n  box-shadow: none;\n  border-top: 1px solid #D9D9D9; }\n\n.con-chat-input form {\n  width: 100%;\n  display: table; }\n\n.con-chat-input-buttons {\n  height: 100px;\n  width: 100%;\n  padding: 25px;\n  box-sizing: border-box;\n  background: #ffffff; }\n\n.con-chat-input-button {\n  background: #0081f9;\n  width: calc(50% - 10px);\n  float: left;\n  height: 50px;\n  color: #fff;\n  line-height: 50px;\n  text-align: center;\n  border-radius: 10px;\n  box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.2);\n  transition: transform 0.5s linear, opacity 0.5s linear; }\n\n.con-chat-input-buttons .con-chat-input-button-active {\n  transform: translate(0, -100px);\n  opacity: 0; }\n\n.con-chat-input-button:nth-child(2n) {\n  margin-left: 20px; }\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n/*iPhone 6/7/8*/\n/*iPhone 6/7/8 Plus*/\n/*iPhone X*/\n",
