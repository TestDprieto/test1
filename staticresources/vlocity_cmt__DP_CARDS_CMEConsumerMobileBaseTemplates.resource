                        "%vlocity_namespace%__HTML__c": "<script>\n    $(document).ready(function(){\n        \n        //Page finished loading so show navbar\n        showPage();\n\t    \n\t    $('.con-chat-button, .con-chat-input-button').one( \"click\", function(e) {\n\t        submitButton(e);\n\t    });\n\t   \n    });\n    \n    function showPage(){\n        if($('.con-navbar-chat').length){\n            $('.con-chat, .con-navbar-chat').css('opacity', '1');\n\t        $('.con-navbar-chat').addClass('con-loaded');\n        }else{\n            setTimeout(function(){showPage();},500);\n        }\n    }\n   \n    //Submit user text\n    function submitText(){\n       $('input').blur();\n       $('.con-chat-input-button').remove();\n       createRight($('#con-chat-input-text').val());\n       $('#con-chat-input-text').val('Ask anything...');\n    }\n    \n    //Submit user button\n    function submitButton(e){\n        $(e.target).addClass('con-chat-input-button-active');\n        createRight($(e.target).html());  \n        setTimeout(function(){\n            $('.con-chat-input-button').remove();\n        }, 500);\n\n    }\n    \n    //Create right chat content\n    function createRight(content){\n        $('.con-chat-window').append('<div class=\"con-chat-text-right con-chat-text-right-active\"><span>' + content + '</span></div>');\n        setTimeout(function(){\n            $('.con-chat-text-right').removeClass('con-chat-text-right-active');\n        },50);\n        $('.con-chat-window').scrollTop($('.con-chat-window')[0].scrollHeight);\n\n        //Temporary responses for demo\n        setTimeout(function(){\n            getNextResponse(content);\n        }, 550);\n    }\n    \n    //Create left chat content\n    function createLeft(content){\n        if(!$('.con-chat-loading').length){\n            $('.con-chat-window').append('<div class=\"con-chat-loading\"><span></span><span></span><span></span></div>');\n            $('.con-chat-window').scrollTop($('.con-chat-window')[0].scrollHeight);\n        }\n        setTimeout(function(){\n            $('.con-chat-loading').remove();\n            $('.con-chat-window').append('<div class=\"con-chat-text-left\">' + content + '</div>');\n            $('.con-chat-window').scrollTop($('.con-chat-window')[0].scrollHeight);\n        }, 2500);\n    }\n    \n    //Create buttons for input\n    function createButtons(content){\n        $.each(content, function(index, value){\n            $('.con-chat-input-buttons').append('<div class=\"con-chat-input-button\">' + value + '</div>');\n        });\n        $('.con-chat-input-button').one( \"click\", function(e) {\n\t        submitButton(e);\n\t    });\n    }\n    \n    //Temporary responses for demo\n    var responseNum = 0;\n    var responsePath = 0;\n    function getNextResponse(){\n        switch(responseNum){\n            case 0: \n                createLeft('Right away.'); \n                createLeft('Would you like to do it over the phone or in-person at a store?');\n                setTimeout(function(){createButtons(['Over the phone', 'In-person']);}, 2500);\n                break;\n            case 1: \n                createLeft('Sounds good.'); \n                createLeft('Looks like your closest store is about a mile away:');\n                createLeft('<div class=\"con-chat-image\"><img src=\"https://vdo-cmt-site.secure.force.com/resource/VLOCMOBCON/images/staticmap.png\"><div class=\"con-chat-image-title\">2055 Moscone Center<span>0.8 mi</span></div></div>');\n                createLeft('Does this location work for you?');\n                setTimeout(function(){createButtons(['Yes', 'No']);}, 2500);\n                break;\n            case 2: \n                createLeft('OK, great. Let\\'s get you scheduled.'); \n                createLeft('These are the earliest available times at this store. Select a time...');\n                setTimeout(function(){createButtons(['Fri 10:00AM', 'Fri 10:30AM']);}, 2500);\n                break;\n            case 3: \n                createLeft('Perfect. We\\'ll see you then!'); \n                break;\n        }\n        responseNum++;\n    }\n    \n</script>\n<div class=\"con-chat\">\n    \n    <div class=\"con-chat-window\">\n        <!-- Usage Overview -->\n        <div class=\"con-chat-details-title\">\n            <div class=\"con-device-chat\">\n                <div class=\"con-device-chat-icon\">\n                    <div class=\"con-device-chat-icon-lines\">\n                        <div class=\"con-device-chat-icon-line1\"></div>\n                        <div class=\"con-device-chat-icon-line2\"></div>\n                        <div class=\"con-device-chat-icon-line3\"></div>\n                        <div class=\"con-device-chat-icon-line4\"></div>\n                        <div class=\"con-device-chat-icon-line5\"></div>\n                    </div>\n                </div>\n            </div>\n            Hi Jenny, how can I help?\n        </div>\n        <div class=\"con-chat-text-left\">\n            You can type in your question, or start with one of these popular topics:\n        </div>\n        \n        <div class=\"con-chat-buttons\">\n            <div class=\"con-chat-button\">\n                Pay your bill\n            </div>\n            <div class=\"con-chat-button\">\n                Check your usage\n            </div>\n            <div class=\"con-chat-button\">\n                Upgrade your device\n            </div>\n            <div class=\"con-chat-button\">\n                Schedule an appointment\n            </div>\n        </div>\n    </div>\n\n    <div class=\"con-chat-input\">\n        <div class=\"con-chat-input-buttons\">\n        </div>\n        <form OnSubmit=\"submitText();\">  \n          <input type=\"text\" value=\"Ask anything...\" id=\"con-chat-input-text\" onfocus=\"this.value='';\"/>\n          <input type=\"submit\" value=\"Search\" style=\"display:none;\"/>\n        </form>\n    </div>\n</div>",
                        "%vlocity_namespace%__ParentID__c": "vloc-mob-con-usage/1/Vlocity Demo Org/1535458577680",
                        "%vlocity_namespace%__Sass__c": "/*Variables*/\n@import \"vloc-mob-con-variables\";\n\n\n/*Usage Details*/\n.con-chat{\n    width: 100vw;\n    height: 100%;\n    background:#FFF;\n    box-sizing:border-box;\n    opacity:0;\n    transition:opacity 1s linear;\n    overflow: hidden;\n}\n.con-chat-details-title {\n    font-size: 16px;\n    color: #000;\n    font-family:Brown-Bold;\n    min-height: 28px;\n    line-height: 50px;\n}\n\n/*Chat Notification*/\n.con-chat .con-device-chat{\n    display:block;\n    padding:0px;\n}\n.con-chat .con-device-chat-icon{\n    float:left;\n    margin-right: 15px;\n    width:50px;\n    height:50px;\n    border-radius:50px;    \n}\n.con-chat .con-device-chat-icon-lines:after{\n    width:50px;\n    height:50px;\n    border-radius:50px;\n}\n.con-chat .con-device-chat-icon-lines{\n    margin:15px auto;\n}\n.con-chat .con-device-chat-icon-line1, .con-chat .con-device-chat-icon-line5{\n    animation: growlarge 4s 1s infinite;\n}\n.con-chat .on-device-chat-icon-line5{\n    animation-delay:4s;\n}\n.con-chat .con-device-chat-icon-line2, .con-chat .con-device-chat-icon-line4{\n    animation: growmedium 4s 3s infinite;    \n}\n.con-chat .con-device-chat-icon-line2{\n    animation-delay:5s;\n}\n.con-chat .con-device-chat-icon-line3{\n    animation: growsmall 4s 2s infinite;\n}\n\n/*Ionic keyboard fixes*/\n.keyboard-up{\n   padding-bottom: 44px !important;\n   overflow:hidden !important;\n}\n\n/*Chat Window*/\n.con-chat-window{\n    height:calc(100% - 235px);\n    position:absolute;\n    top:60px;\n    left:0px;\n    width:100%;\n    padding:25px;\n    box-sizing:border-box;\n    overflow:scroll;\n}\n.con-chat-text-left{\n    width:100%;\n    padding-right:20%;\n    float:left;\n    margin-bottom: 10px;\n}\n.con-chat-text-right{\n    width:100%;\n    padding-left:20%;\n    box-sizing:border-box;\n    float:right;\n    margin-bottom:25px;\n    margin-top: 15px;\n    position:relative;\n    transition:opacity 0.5s linear;\n}\n.con-chat-text-right-active{\n    opacity:0;\n}\n.con-chat-text-right span{\n    width:auto;\n    float:right;\n    background:$theme-colour;\n    color:#fff;\n    border-radius:10px;\n    padding: 20px;\n    border-bottom-right-radius:0px;\n    position:relative;\n    display:block;\n}\n.con-chat-text-right span:after {\n    content: ' ';\n    width: 10px;\n    height: 10px;\n    border-left: 10px solid transparent;\n    border-right: 10px solid transparent;\n    border-bottom: 10px solid $theme-colour;\n    position: absolute;\n    right: -5px;\n    bottom: 0px;\n}\n.con-chat-button{\n    color:$theme-colour;\n    width: 100%;\n    height: 40px;\n    text-align: center;\n    line-height: 38px;\n    border: 1px solid #D9D9D9;\n    margin-bottom: 10px;\n    border-radius: 5px;\n    box-sizing: border-box;\n    font-family: Brown-Bold;\n    float:left;\n}\n.con-chat-buttons{\n    margin-top: 10px;\n    display:table;\n    float:left;\n}\n.con-chat-loading{\n    float:left;\n    width:100%;\n    height:20px;\n}\n.con-chat-loading span{\n    height:8px;\n    width:8px;\n    background:#D9D9D9;\n    margin-right:8px;\n    border-radius:8px;\n    display:block;\n    float:left;\n    animation: loading 1s infinite;\n    margin-top:10px;\n}\n.con-chat-loading span:nth-child(n+2){\n    animation: loading 1s 0.2s infinite;\n}\n.con-chat-loading span:nth-child(n+3){\n    animation: loading 1s 0.4s infinite;\n}\n\n@keyframes loading {\n    0% {transform:translate3d(0,0,0);}\n    33% {transform:translate3d(0,-5px,0);}\n    66% {transform:translate3d(0,0,0);}\n    100% {transform:translate3d(0,0,0);}\n}\n\n/*Chat Image Responses*/\n.con-chat-image {\n    border-radius: 10px;\n    overflow: hidden;\n    border: 1px solid #d4d4d4;\n    min-height: 146px;\n}\n.con-chat-image-title {\n    height: 40px;\n    padding: 8px 15px;\n    color: $theme-colour;\n}\n.con-chat-image-title span {\n    float: right;\n    color: $subtitle-colour;\n}\n\n/*Chat Input*/\n.con-chat-input{\n    position:absolute;\n    bottom:0px;\n    left:0px;\n    height:160px;\n    line-height:60px;\n    box-sizing:border-box;\n    width:100vw;\n}\n.con-chat-input input[type=text]{\n    width: 100%;\n    padding: 0px 25px;\n    height: 60px;\n    color:#D9D9D9;\n    font-family:Brown-Light;\n    -webkit-appearance: none;\n    border-radius:0px;\n    background:#fff;\n    box-shadow:none;\n    border-top:1px solid #D9D9D9;\n}\n.con-chat-input form{\n    width:100%;\n    display:table;\n}\n.con-chat-input-buttons{\n    height:100px;\n    width:100%;\n    padding:25px;\n    box-sizing:border-box;\n    background:#ffffff;\n}\n.con-chat-input-button{\n    background:$theme-colour;\n    width:calc(50% - 10px);\n    float:left;\n    height:50px;\n    color:#fff;\n    line-height:50px;\n    text-align:center;\n    border-radius:10px;\n    box-shadow:0px 0px 25px rgba(0,0,0,0.2);\n    transition:transform 0.5s linear, opacity 0.5s linear;\n}\n.con-chat-input-buttons .con-chat-input-button-active{\n    transform: translate(0, -100px);\n    opacity:0;\n}\n.con-chat-input-button:nth-child(2n){\n    margin-left:20px;\n}\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n@media only screen \n    and (max-device-height : 640px) {\n \n}\n/*iPhone 6/7/8*/\n@media only screen \n    and (max-device-height : 667px) {\n\n}\n/*iPhone 6/7/8 Plus*/\n@media only screen \n    and (min-device-height : 668px) and (max-device-height : 737px) {\n\n}\n/*iPhone X*/\n@media only screen \n    and (min-device-height : 738px) {\n\n}",
