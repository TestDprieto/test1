                        "%vlocity_namespace%__Sass__c": "/*Variables*/\n@import \"vloc-mob-con-variables\";\n\n\n\n/*Device Details*/\n.con-device-details{\n    width:100%;\n    height:auto;\n    background:#FFF;\n    position:relative;\n    z-index:1;\n    box-shadow:0px -60px 200px rgba(0,0,0,1);\n    border-radius: 20px 20px 0px 0px;\n}   \n.con-device-details-wrapper{\n    transition:transform 0.5s linear;\n    transform:translateY(60vh);\n    position: relative;\n    z-index: 1;\n}\n.con-device-details-expanded{\n    transform:translateY(0) !important;\n}\n.con-device-details-border{\n    border-bottom:1px solid #e9ecfe; \n    width:calc(100vw - 50px);\n    margin:auto;\n}\n.con-device-details-title{\n    text-align:center;\n    font-size:20px;\n    font-family:Brown-Bold;\n    width:100%;\n    padding:0px 25px 0px 25px;\n    position: relative;\n    overflow:hidden;\n    box-sizing:border-box;\n    height:0px;\n    transition:height 0.25s linear, opacity 1s linear, margin-bottom 0.25s linear;\n    opacity:0;\n    margin-bottom:-25px;\n}\n.con-device-details-title span{\n    font-family:fontello;\n    position: absolute;\n    left: 25px;\n}\n.con-device-details-expanded .con-device-details-title{\n    height:30px;\n    opacity:1;\n    margin-bottom:0px;\n}\n\n/*Device Details Section*/\n.con-device-details-section{\n    padding: 25px;\n    text-align:left; \n}\n.con-device-details-section-title{\n    font-size:24px;\n    color:#000;\n    margin-bottom:25px;\n    font-weight:bold;\n    min-height: 28px;\n}\n.con-device-details-section-subtitle{\n    color: $subtitle-colour;\n    font-size: 14px;\n    width: 100%;\n    margin-top: 10px;\n    font-family: Brown-Light;\n}\n.con-device-details-section-button{\n    color: #ffffff;\n    font-size: 14px;\n    background: $theme-colour;\n    border-radius: 5px;\n    line-height: 40px;\n    width: 100%;\n    height: 40px;\n    display: table;\n    padding: 0px 25px;\n    text-align:center;\n    margin-top:25px;\n}\n\n/*Device Graphs*/\n\n/*Bar Graphs*/\n.con-device-details-bar-graph{\n    width:100%;\n    display:table;\n    margin-bottom:15px;\n}\n.con-device-details-bar-graph-title{\n    float:left;\n    width:65%;\n}\n.con-device-details-bar-graph-subtitle{\n    float:right;\n    width:35%;\n    text-align:right;\n}\n.con-device-details-bar-graph-chart{\n    width:100%;\n    height:10px;\n    float:left;\n    margin-top:10px;\n    background:#e9ecfe;\n}\n.con-device-details-bar-graph-chart-fill{\n    height:10px;\n    background: $theme-colour;\n    transition:width 1s linear;\n}\n.swiper-container:not(.swiper-container-active) .con-device-details-bar-graph-chart-fill{\n    width:0% !important;\n}\n\n/*Donut Graphs*/\n.con-device-details-donut-graph{\n    display:table;\n    width:100%;\n}\n.con-device-details-donut-graph-chart{\n    width:33%;\n    float:left;\n    text-align:center;\n    position:relative;\n}\n.con-device-details-donut-graph svg{\n    transform: rotate(-90deg);\n    margin: auto;\n    display: block;\n}\n.con-device-details-donut-graph .circle_animation {\n    stroke-dasharray: 190;\n    stroke: $theme-colour;\n    transition: stroke-dashoffset 1s linear;\n}\n.con-device-details-donut-graph .circle_empty{\n    stroke: #e9ecfe !important;\n}\n.swiper-container:not(.swiper-container-active) .con-device-details-donut-graph .circle_animation{\n    stroke-dashoffset: 190;\n}\n.con-device-details-donut-graph-title{\n    color: $subtitle-colour;\n    margin-bottom:5px;\n    font-family:Brown-Light;\n}\n.con-device-details-donut-graph-subtitle{\n    margin-top:5px;\n    font-family: Brown-Light;\n}\n.con-device-details-donut-graph-data{\n    position: absolute;\n    top: 50px;\n    left: 50%;\n    margin-left: -20px;\n    width: 40px;\n    text-align: center;\n    font-size: 16px;\n}\n.con-icon-infinity{\n    color:#e9ecfe;\n    font-family: fontello;\n    font-size: 26px;\n}\n\n/*Device Perks*/\n.con-device-details-perk{\n    text-align:left;\n}\n.con-device-details-perk-icon{\n    color:$theme-colour;\n    font-family: fontello;\n    font-size: 28px;  \n    width:50px;\n    float:left;\n    line-height: 45px;\n}\n.con-device-details-perk-title{\n    font-size: 16px;\n    margin-bottom: 5px;\n    margin-left: 50px;\n    font-family: Brown-Bold;\n}\n.con-device-details-perk-subtitle{\n    font-size:14px;\n    margin-bottom:15px;\n    margin-left:50px;\n    color:$subtitle-colour;\n    font-family: Brown-Light;\n}\n\n/*Device Actions*/\n.con-device-details-action{\n  font-size:16px;  \n  color:$theme-colour;\n  padding:25px 0px;\n  font-family: Brown-Bold;\n}\n.con-device-details-action span{\n    float:right;\n    font-size:18px;\n    line-height:20px;\n    font-family: fontello;\n}\n\n/*Device Notifications*/\n.con-device-details-notification{\n    width:100vw;\n    height:90px;\n    box-shadow:0px 0px 25px rgba(0,0,0,0.15);\n    margin-top: 25px;\n    position:relative;\n    margin-left:-25px;\n    padding:0px 25px;\n    box-sizing:border-box;\n}\n.con-device-details-notification-icon{\n    width: 35px;\n    height: 35px;\n    float: left;\n    border-radius: 40px;\n    border: 1.5px solid $theme-colour;\n    box-shadow: 0px 0px 13px $theme-colour;\n    margin: 16px 15px 25px 0px;\n}\n.con-device-details-notification-title{\n    margin-bottom: 5px;\n    margin-left: 50px;\n    padding-top: 11px;\n}\n.con-device-details-notification-action{\n    color: $theme-colour;\n    font-family:Brown-Bold;\n}\n.con-device-details-notification-action span{\n    font-family:fontello;\n    font-size: 12px;\n    margin-left: 5px;\n}\n.con-device-details-notification-icon-lines{\n    margin:12px auto;\n    display:table;\n}\n.con-device-details-notification-icon-lines > div{\n    float: left;\n    width: 1px;\n    background: $theme-colour;\n    border-radius: 10px;\n}\n.con-device-details-notification-icon-line1, .con-device-details-notification-icon-line5{\n    height: 3px;\n    margin: 2.5px 1px;\n    opacity: 0.6;\n}\n.swiper-slide-active .con-device-details-notification-icon-line1, .swiper-slide-active .con-device-details-notification-icon-line5{\n    animation: growlarge 4s 1s infinite;\n}\n.swiper-slide-active .con-device-details-notification-icon-line5{\n    animation-delay:4s;\n}\n.con-device-details-notification-icon-line2, .con-device-details-notification-icon-line4{\n    height: 5px;\n    margin: 1.5px 1px;\n    opacity: 0.7;\n}\n.swiper-slide-active .con-device-details-notification-icon-line2, .swiper-slide-active .con-device-details-notification-icon-line4{\n    animation: growmedium 4s 3s infinite;    \n}\n.swiper-slide-active .con-device-details-notification-icon-line2{\n    animation-delay:5s;\n}\n.con-device-details-notification-icon-line3{\n    height: 7px;\n    margin: 0.5px 1px;\n    opacity: 0.8;\n    \n}\n.swiper-slide-active .con-device-details-notification-icon-line3{\n    animation: growsmall 4s 2s infinite;\n}\n\n/*Device Grabber*/\n.con-device-grabber{\n    width:100%;\n    height:40px;\n    float:left;\n    background:#FFF;\n    border-radius:15px;\n    transition:border-radius 0.5s linear;\n}\n.con-device-grabber-bar{\n    margin:auto;\n    width:40px;\n    height:4px;\n    background:#e9ecfe; /*#D9D9D9*/\n    border-radius:10px;\n    top:10px;\n    position:relative;\n    transition:opacity 0.5s linear;\n}\n.con-opacity-hidden{\n    opacity:0;\n    pointer-events:none !important;\n}\n.grabberup{\n    border-radius:0px\n}\n.grabberup .con-device-grabber-bar{\n    opacity:0;\n}\n\n/*Device Title*/\n.con-device-title{\n    color: $white-text-colour;\n    font-size: 30px;\n    position: absolute;\n    top: calc(60vh - 65px);\n    left: 25px;\n    z-index: 2;\n    height: 40px;\n    line-height: 40PX;\n}\n\n/*Device Image*/\n.con-device-image{\n    top:80px;\n    width:100%;\n    padding:40px 30px 30px 30px;\n    box-sizing:border-box;\n    position:absolute;\n    left:0px;\n    background: -webkit-linear-gradient(top, rgba(255,255,255,0) 0%,#ffffff 100%);\n    transition: opacity 0.5s linear;\n    height: calc(100vh - 80px);\n    overflow: hidden;\n\n}\n.con-device-image-small{\n    opacity:0;\n}\n.con-device-image-small img{\n    transform: translate3d(-65px, 30vh, 0) scale(0.4) skew(0deg,-5deg);\n}\n.con-device-image img{\n    margin:auto;\n    display:block;\n    transition:transform 0.5s linear;\n}\n\n/*Device Shadow*/\n.con-device-shadow{\n    width:100%;\n    height:50vh;\n    background: linear-gradient(to bottom, rgba(0,0,0,0) 1%, rgba(0,0,0,0.85) 50%, #000000 100%); \n    position:absolute;\n    top:50vh;\n    left:0px;\n}\n\n/*Device Chat */\n.con-device-chat{\n    width:100%;\n    padding-top:100px;\n    padding-bottom:50px;\n    display:none;\n}\n.con-device-details-expanded .con-device-chat{\n    display:block;\n}\n.con-device-chat-icon{\n    width: 60px;\n    height: 60px;\n    border-radius: 60px;\n    border: 1.5px solid $theme-colour;\n    box-shadow: 0px 0px 13px $theme-colour;\n    margin: 0px auto 15px auto;\n    background:#FFFFFF;\n    position:relative;\n}\n.con-device-chat-icon-lines{\n    margin:20px auto;\n    display:table;\n}\n.con-device-chat-icon-lines > div{\n    float: left;\n    width: 2px;\n    background: $theme-colour;\n    border-radius: 10px;\n}\n.con-device-chat-icon-line1, .con-device-chat-icon-line5{\n    height: 5px;\n    margin: 5px 1.5px;\n    opacity: 0.6;\n    animation: growlarge 4s 1s infinite;\n}\n.con-device-chat-icon-line5{\n    animation-delay:4s;\n}\n.con-device-chat-icon-line2, .con-device-chat-icon-line4{\n    height: 9px;\n    margin: 3px 1.5px;\n    opacity: 0.7;\n    animation: growmedium 4s 3s infinite;\n}\n.con-device-chat-icon-line2{\n    animation-delay:5s;\n}\n.con-device-chat-icon-line3{\n    height: 13px;\n    margin: 1px 1.5px;\n    opacity: 0.8;\n    animation: growsmall 4s 2s infinite;\n}\n.con-device-chat-icon-lines:after{\n    content:' ';\n    width: 60px;\n    height: 60px;\n    border-radius: 60px;\n    background: $theme-colour;\n    position: absolute;\n    top: -1.5px;\n    left: -1.5px;\n    z-index:-1;\n    animation: spin 6s linear infinite;\n    opacity: 0.5;\n}\n.con-device-chat-subtitle{\n    color:$theme-colour;\n    text-align:center;\n    width:100%;\n}\n.con-device-chat-title{\n    color:$theme-colour;\n    font-family:Brown-Bold;\n    text-align:center;\n    width:100%;\n}\n\n/*Chat Bot Icon Animations*/\n@keyframes growlarge {\n    0% {transform:scaleY(1);}\n    15% {transform:scaleY(1.75);}\n    30% {transform:scaleY(0.6);}\n    45% {transform:scaleY(1.5);}\n    60% {transform:scaleY(0.7);}\n    75% {transform:scaleY(2);}\n    90%,100% {transform:scaleY(1);}\n}\n@keyframes growmedium {\n    0% {transform:scaleY(1);}\n    15% {transform:scaleY(1.3);}\n    30% {transform:scaleY(0.85);}\n    45% {transform:scaleY(1.15);}\n    60% {transform:scaleY(0.7);}\n    75% {transform:scaleY(1.45);}\n    90%,100% {transform:scaleY(1);}\n}\n@keyframes growsmall {\n    0% {transform:scaleY(1);}\n    15% {transform:scaleY(1.25);}\n    30% {transform:scaleY(0.9);}\n    45% {transform:scaleY(1.1);}\n    60% {transform:scaleY(0.85);}\n    75% {transform:scaleY(1.4);}\n    90%,100% {transform:scaleY(1);}\n}\n@keyframes spin {\n    0% {transform:rotate(0deg) scale(1, 1);}\n    5% {transform:rotate(180deg) scale(1.06, 1.1);}\n    10% {transform:rotate(360deg) scale(1.02, 1);}\n    15% {transform:rotate(540deg) scale(1, 1);}\n    20% {transform:rotate(720deg) scale(1, 1);}\n    25% {transform:rotate(900deg) scale(1.09, 1.15);}\n    30% {transform:rotate(1080deg) scale(1.02, 1);}\n    35% {transform:rotate(1260deg) scale(1, 1);}\n    40% {transform:rotate(1440deg) scale(1, 1);}\n    45% {transform:rotate(1620deg) scale(1.12, 1.19);}\n    50% {transform:rotate(1800deg) scale(1.02, 1);}\n    55% {transform:rotate(1980deg) scale(1, 1);}\n    100% {transform:rotate(0deg) scale(1, 1);}\n}\n\n/*Product Browse*/\n.con-shop-browse{\n    display:table;\n    min-height: 500px;\n}\n.con-shop-browse-product{\n    width:calc(50% - 12.5px);\n    display:table;\n    float:left;\n    margin-bottom: 25px;\n}\n.con-shop-browse-product:nth-child(odd){\n    margin-right:25px;\n}\n.con-shop-browse-product-background{\n    background:$circle-colour-2;\n    height:150px;\n    width:150px;\n    border-radius:100%;\n    padding-top: 10px;\n    margin-bottom: 25px;\n}\n.con-shop-browse-product:nth-child(4n+1) .con-shop-browse-product-background, .con-shop-browse-product:nth-child(4n+4) .con-shop-browse-product-background {\n    background: $circle-colour-1;\n}\n.con-shop-browse-product-image img {\n    height: 160px;\n    margin: auto;\n    display: block;\n    width: auto;\n}\n.con-shop-browse-product-discount-percentage {\n    color: $discount-colour;\n    min-height:20px;\n    min-width:20px;\n}\n.con-shop-browse-product-name {\n    font-size: 16px;\n    color: $theme-colour;\n    font-family: Brown-Bold;\n    min-height: 20px;\n    max-height: 40px;\n    overflow: hidden;\n    margin-bottom: 10px;\n    /* white-space: nowrap; */\n    /* text-overflow: ellipsis; */\n}\n.con-shop-browse-product-price {\n    font-size: 16px;\n    width: 50%;\n    float: left;\n    margin-bottom: 10px;\n    font-family: Brown-Bold;\n}\n.con-shop-browse-product-discount-price {\n    text-decoration: line-through;\n    color: $subtitle-colour;\n    float: right;\n    width: 50%;\n    height: 20px;\n    margin-bottom: 10px;\n}\n.con-shop-browse-product-attribute-more {\n    width: 50%;\n    float: right;\n    font-size: 12px;\n    font-family: Brown-Light;\n    line-height:18px;\n    height: 18px;\n}\n.con-shop-browse-product-attribute-colour {\n    width: 18px;\n    height: 18px;\n    float: left;\n    border-radius: 20px;\n    margin-right: 5px;\n}\n.con-shop-browse-product-attribute-colour-border {\n    border: 1px solid #e9ecfe;\n}\n\n/*Device Specific Styling*/\n/*iPhone 5/SE*/\n@media only screen \n    and (max-device-height : 640px) {\n    .con-device-details-donut-graph{\n        display:none;   \n    }\n    .con-device-details-wrapper {\n        transform: translateY(calc(100vh - 250px));\n    }\n    .con-device-title {\n        top: calc(100vh - 310px);\n    }\n}\n/*iPhone 6/7/8*/\n@media only screen \n    and (min-device-height : 641px) and (max-device-height : 667px) {\n    .con-device-details-bar-graph{\n        display:none;   \n    }\n    .con-device-details-wrapper {\n        transform: translateY(calc(100vh - 260px));\n    }\n    .con-device-title {\n        top: calc(100vh - 320px);\n    }\n}\n/*iPhone 6/7/8 Plus*/\n@media only screen \n    and (min-device-height : 668px) and (max-device-height : 737px) {\n    .con-device-details-donut-graph{\n        display:none;   \n    }\n    .con-device-details-wrapper {\n        transform: translateY(calc(100vh - 340px));\n    }\n    .con-device-title {\n        top: calc(100vh - 405px);\n    }\n}\n/*iPhone X*/\n@media only screen \n    and (min-device-height : 738px) {\n    .con-device-details-donut-graph{\n        display:none;   \n    }\n    .con-device-details-wrapper {\n        transform: translateY(calc(100vh - 340px));\n    }\n    .con-device-title {\n        top: calc(100vh - 405px);\n    }\n}",
