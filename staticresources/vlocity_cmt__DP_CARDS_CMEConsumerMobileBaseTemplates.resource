d: #FFF;\n}\n.con-usage-tab{\n    height: 50px;\n    width: 20%;\n    padding: 0px 12.5px;\n    line-height: 56px;\n    margin: 0px 0px 0px 10%;\n    font-size: 16px;\n    color:$subtitle-colour;\n    display: inline-block;\n    text-align: center;\n}\n.con-usage-tab-active{\n    border-bottom:3px solid #000;\n    color:#000;\n}\n.con-usage-tab-content{\n    pointer-events:none;\n    position:absolute;\n    background: #fff;\n    overflow: hidden;\n    width: 100%;\n    pointer-events:all;\n}\n.con-usage-tab-content-active > div{\n    opacity:1;\n}",
