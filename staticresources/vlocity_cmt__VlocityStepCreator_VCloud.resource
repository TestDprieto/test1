        {
            "ExternalId": "createDesignatedUserForConnectedAppFirstInstall",
            "UniqueId": "createDesignatedUserForConnectedAppFirstInstall",
            "Name": "Create Designated User for Connected App (New Installation)",
            "Priority": "None",
            "Tags": "CLM VCloud (Beta)",
            "VersionIntroduced": "",
            "IsActive": true,
            "Type": "Manual",
            "StepFrequency": "First Install",
            "Instruction": "<ol><li>Go to Setup.</li><li>In the Quick Find box, enter Users.</li><li>Click Users.</li><li>Click New User.</li><li>Supply the following data:<ol><li>User License: Salesforce</li><li>Profile: System Administrator</li></ol></li><li>Define the other required fields.<ol><li>Last Name</li><li>Alias</li><li>Email</li><li>Username</li><li>Nickname</li><li>Role</li></ol></li><li>Click Save.</li></ol>",
            "CustomInputLabel": "",
            "CustomInputValue": "",
            "Options": "",
            "ArgumentList": "",
            "StepCommand": "",
