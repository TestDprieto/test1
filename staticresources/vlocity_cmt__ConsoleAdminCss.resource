body{margin:0;padding:0}#console-header{background-color:#000;height:60px}#console-header .vlocity-logo{height:35px;width:112px;margin-top:13px;margin-left:10px}#console-header a.backtosf{color:#fff;display:inline-block;margin-top:23px;margin-right:10px}#console-header a.backtosf .slds-icon_container{position:relative;bottom:2px;margin-left:8px}#console-header a.backtosf:active,#console-header a.backtosf:hover,#console-header a.backtosf:link,#console-header a.backtosf:visited{text-decoration:none}.via-slds .tabset{background-color:#f4f6f9}.via-slds .nav-tabs{display:table;height:31px}.via-slds .nav-tabs>li{color:#555;cursor:pointer;border-right:1px solid #ccc;display:inline-block;font-size:14px;height:31px;margin:0}.via-slds .nav-tabs>li:hover{background-color:#fff}.via-slds .console-navs>li.object{color:#333}.via-slds .console-navs>li.object:hover{background-color:#f4f6f9}.via-slds .console-navs>li.object div{font-weight:700;width:150px}.via-slds .console-navs>li.object div:hover{color:inherit}.via-slds .nav-tabs>li.active{background-color:#fff;height:32px;margin:0}.via-slds .nav-tabs>li>div .glyphicon{color:#888;font-size:16px;position:absolute;top:4px;right:3px}.via-slds .nav-tabs>li>div .glyphicon:hover{color:#000;cursor:pointer}.via-slds .nav-tabs>li>div{margin:0;padding:5px 38px 5px 10px;position:relative;text-transform:none}.via-slds .nav-tabs>li>div:hover{margin:0}.via-slds .nav-tabs>li>div.new{padding:3px 10px}.via-slds .nav-tabs>li.active>div,.via-slds .nav-tabs>li.active>div:hover{color:#000;margin:0}.via-slds .btn-close-tab{color:#444;position:absolute;right:5px;top:7px}.via-slds .console-tab,.via-slds .tab-pane{height:inherit}.via-slds .console-tab-content{background-color:#fff;border-top:1px solid rgba(0,0,0,.2);overflow:hidden}.via-slds .console-tab-content iframe.object-tab{border:none;height:100%;min-width:100%;overflow-y:auto}.via-slds .console-dashboard{height:100%;overflow-y:auto;padding:10px}.via-slds .console-dashboard .module{color:#000;margin:5px 0 20px;max-width:33.3333333333%}.via-slds .console-dashboard .module .slds-card{background:#f7f9fb;box-shadow:none}.via-slds .console-dashboard .module .slds-card__header{padding-left:.7rem;padding-right:.7rem}.via-slds .console-dashboard .module .slds-card__body{margin-bottom:0}.via-slds .console-dashboard .module ul.console-objects{background-color:#fff;border-bottom-left-radius:4px;border-bottom-right-radius:4px}.via-slds .console-dashboard .module ul.console-objects li{padding:8px 12px}.via-slds .console-dashboard .module ul.console-objects li:hover{background-color:#f4f6f9}.via-slds .console-dashboard .module ul.console-objects li .action{cursor:pointer;margin-left:12px}.via-slds .console-dashboard .module .slds-badge-comingsoon{background-color:#d2e4f4;color:#444;margin-left:10px}.via-slds .toaster-container{left:calc(100% - 32rem);right:0}.via-slds .toaster-container .slds-notify.slds-notify--toast{word-break:break-word}