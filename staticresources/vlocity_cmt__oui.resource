    .vlocity .steps-chart {
        margin: 0;
    }
    .vlocity .vlc-sidebar-instruction {
        float: right;
    }
    /* hide horizontal components */
    .vlocity .vlc-omni-step .hr,
    .vlocity .vlc-omni-step>label,
    .vlocity .vlc-omni-step:last-child > .step-connector {
        display: none !important;
    }
    /* style vertical components */
    .vlocity .steps-chart .steps-container,
    .vlocity .vlc-omni-step,
    .vlocity .vlc-sidebar-wrapper,
    .vlocity .vlc-omni-step > .step-connector,
    .vlocity .vlc-omni-step>div>label {
        display: block;
    }
    .vlocity .vlc-omni-step {
        width: 100%;
        max-width: 100%;
        min-height: 40px;
    }
    .vlocity .vlc-omni-step>div {
        display: table;
    }
    .vlocity .vlc-omni-step>div>label {
