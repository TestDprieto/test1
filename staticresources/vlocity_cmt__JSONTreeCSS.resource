  cursor: pointer;
}
.vlc-json-tree-dark.vlc-json-tree-row .constructor-name {
  cursor: pointer;
}
.vlc-json-tree-dark.vlc-json-tree-row .toggler {
  font-size: 0.8em;
  line-height: 1.2em;
  vertical-align: middle;
  opacity: 0.6;
