filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1797c0', endColorstr='#0d79a9',GradientType=0 ); /* IE6-9 */
}

.vlocity-minicart .row > div > button:hover{
background: #0094ba; /* Old browsers */
