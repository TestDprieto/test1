filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0094ba', endColorstr='#0074a3',GradientType=0 ); /* IE6-9 */
}


.vlocity-minicart .row > div > button.vlocity-disabled{   
margin: 0 auto;
height: 35px;
text-align: center;
line-height: 30px;
text-transform: uppercase;
font-size: 12px;
letter-spacing: 1px;
border-radius: 4px; 
width: 100%;
margin-top: 10px;
background-image: none;
color: #dadada;
border: 1px solid #dadada;
text-align: center;
background: transparent;
box-shadow: none;
border-bottom-color: #dadada;
text-shadow: none;
}


