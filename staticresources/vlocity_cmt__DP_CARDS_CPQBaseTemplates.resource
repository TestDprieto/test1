                        "%vlocity_namespace%__HTML__c": "<div class=\"cpq-discount-list\">\n    <!-- CARDS -->\n    <div class=\"slds-grid cpq-discount-grid cpq-discount-list-vertical cpq-discount-list-col-stretch\"\n        ng-if=\"!importedScope.$parent.$parent.filters.show\">\n        <div class=\"slds-spinner_container\" ng-hide=\"isLoaded\">\n            <div class=\"slds-spinner_brand slds-spinner slds-spinner_medium\" aria-hidden=\"false\" role=\"alert\">\n                <div class=\"slds-spinner__dot-a\"></div>\n                <div class=\"slds-spinner__dot-b\"></div>\n            </div>\n        </div>\n\n        <vloc-card ng-repeat=\"card in cards |filter: searchFunc(card)\" \n            ctrl=\"CPQDiscountsItemController\" \n            ng-model=\"card\" data=\"card\" records=\"records\" \n            index=\"{{$index}}\" session-id=\"{{sessionId}}\" \n            ng-if=\"isLoaded\">\n        </vloc-card>\n\n        <div class=\"slds-text-align_center slds-m-vertical_medium cpq-no-discount-msg\" ng-show=\"!records && isLoaded\">\n            {{::importedScope.customLabels.CPQDiscountsNotAvailable}}\n        </div>\n\n        <div class=\"cpq-next-product slds-col slds-text-align_center slds-m-vertical_medium\"  ng-if=\"session.nextDiscounts\">\n            <div class=\"slds-spinner_container\" ng-show=\"loadingMore\">\n                <div class=\"slds-spinner_brand slds-spinner slds-spinner_small\" aria-hidden=\"false\" role=\"alert\">\n                    <div class=\"slds-spinner__dot-a\"></div>\n                    <div class=\"slds-spinner__dot-b\"></div>\n                </div>\n            </div>\n            <a ng-click=\"importedScope.nextPageDiscounts()\" ng-hide=\"loadingMore || !isLoaded\">\n              {{::importedScope.customLabels.CPQLoadMore}}\n            </a>\n        </div>\n    </div>\n</div>",
