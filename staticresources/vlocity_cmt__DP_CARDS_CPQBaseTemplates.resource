                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-product-cart-item-create-discount {\n            .#{$namespace}-custom-discount-title {\n                text-transform: uppercase;\n                font-weight: 300;\n                border-bottom: 1px solid #dddbda;\n            }\n            .#{$namespace}-new-discount-container {\n                .#{$namespace}-discount-search-container {\n                    .#{$namespace}-discount-badge {\n                        display: inline-block;\n                        border : 1px solid #ecebea;\n                        color:#006fd2;\n                        background:none;\n                        .#{$namespace}-discount-close-icon-color {\n                            color:#706e6b;\n                        }\n                    }\n                }\n                .#{$namespace}-discount-month-input {\n                    width: 14%;\n                }\n                .#{$namespace}-discount-slds-radio {\n                    display: inline-block;\n                }\n                .#{$namespace}-discount-date-input {\n                    width: 30%;\n                }\n                .#{$namespace}-discount-form_horizontal {\n                    text-align: left;\n                }\n                .#{$namespace}-discount-label {\n                    text-align: left;\n                }\n            }\n            input::-webkit-input-placeholder {\n                color: $middle-dark-gray;\n            }\n            .#{$namespace}-error-msg {\n                color: $error;\n            }\n            .#{$namespace}-success-msg {\n                color: $success;\n            }\n            .#{$namespace}-cell-pricing-modal__border {\n                border-top: 2px solid #d8dde6;\n            }\n            .#{$namespace}-discount-dropdown {\n                .#{$namespace}-pricing-picklist__label  {\n                    width:20.5rem;\n                }  \n            }\n            /* Overwrite slds-picklist width */\n            .#{$namespace}-pricing-picklist {\n                .#{$namespace}-discount-negative-mark {\n                    font-weight : 700;\n                    font-size: 15px;\n                }\n                width:100%;\n                .#{$namespace}-discount-input-filed {\n                    width: 14%;\n                }\n                .#{$namespace}-pricing-picklist__label,.#{$namespace}-pricing-picklist-dropdown {\n                    width:20.5rem;\n                } \n                .#{$namespace}-time-plan-picklist-value__label {\n                    width:8rem;\n                }\n                .#{$namespace}-pricing-picklist-value__label{\n                    width:5rem;\n                }\n            }\n            .#{$namespace}-pricing-mandatory-input {\n                border: 1px solid $error;\n            }\n            .#{$namespace}-pricing-block-center {\n                margin: auto;\n            }\n        }\n    }\n}",
