                        "%vlocity_namespace%__HTML__c": "<div class=\"cpq-input-custom-template-sample slds-m-bottom_medium\" ng-controller=\"customActionsController\">\n    <!-- $parent.$parent.records contains the reference to field object passed from VDF -->\n    <div class=\"slds-form-element\">\n        <label class=\"slds-form-element__label\">\n            <abbr class=\"slds-required\" title=\"required\" ng-if=\"field.required\">*</abbr>\n            {{field.label}}\n        </label>\n        <div class=\"slds-form-element__control slds-input-has-fixed-addon\">\n            <input class=\"slds-input\" type=\"text\" ng-model=\"field.userValues\"/>\n        </div>\n    </div>\n    <div class=\"slds-grid slds-m-top_small slds-grid_pull-padded\">\n        <div class=\"slds-col_padded\">\n            <button type=\"button\" class=\"slds-button slds-button_neutral\" ng-click=\"autoGenerate();\">Auto generate</button>\n        </div>\n        <div class=\"slds-col_padded\">\n            <button type=\"button\" class=\"slds-button slds-button_neutral\" ng-click=\"reset();\">Reset</button>\n        </div>\n    </div>\n    <div class=\"slds-hide slds-m-top_small\" ng-class=\"{'slds-show_inline-block': field.userValues}\">\n        Your input value is <b>{{field.userValues}}</b>\n    </div>\n</div>",
