                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-card-mixin\";\n@import \"cpq-mixin\";\n\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n@include cardMixin($namespace);\n\n/*** STYLES ***/\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-discount-item {\n            border-bottom: $cpq-product-item-padding solid $cpq-page-bg-color;\n            .#{$namespace}-discount-label {\n                text-transform: uppercase;\n                background: $discount-type-label-background;\n                font-size:$cpq-cart-item-attr-label-font-size;\n                color: $cpq-product-container-bg-color;\n                padding: 3px 10px;\n                .#{$namespace}-discount-date-label {\n                    text-transform: none;\n                    float: right;\n                    font-size: 10px;\n                    font-weight: 300;\n                }\n            }\n\n            .#{$namespace}-discount-container {\n                padding: $cpq-padding-large-vertical 0px;\n                .#{$namespace}-discount-name {\n                    font-weight: $cpq-headings-font-weight;\n                    line-height: 30px;\n                }\n                .#{$namespace}-discount-link {\n                    line-height: 30px;\n                    color: $cpq-product-link-color;\n                }\n                .#{$namespace}-discount-price {\n                    .cpq-discount-timeplan-label {\n                        font-weight: $cpq-cart-item-product-group-font-weight;\n                    }\n                    .cpq-discount-info-wrap {\n                        font-weight: $cpq-headings-font-weight;\n                        color: $cpq-discount-info-color;\n                        font-size: $cpq-discount-font-size;\n                        .cpq-discount-info-label {\n                            font-size: $cpq-cart-item-attr-label-font-size;\n                        }\n                    }\n                    padding-right: $cpq-padding-large-horizontal;\n                    color: $dark-gray;\n                    &:last-child {\n                        padding-bottom:$cpq-padding-large-vertical;\n                    }\n                    .#{$namespace}-discount-label {\n                      font-size: $cpq-cart-item-attr-label-font-size;\n                    }\n                }\n                .#{$namespace}-discount-price .#{$namespace}-item-product-messages {\n                    font-size: 95%;\n\n                    .#{$namespace}-error-msg {\n                        color: $error;\n                    }\n                    .#{$namespace}-warning-msg {\n                        color: $warning;\n                    }\n                }\n            }\n            \n            .#{$namespace}-discount-actions {\n                padding-top:$cpq-padding-large-vertical;\n                padding-bottom:$cpq-padding-large-vertical; \n                \n                .#{$namespace}-add-button {\n                    padding-left:$cpq-padding-small-horizontal;\n                    padding-right:$cpq-padding-small-horizontal;\n               }\n            }\n        }\n    }\n}\n\n[dir=rtl] .vlocity {\n    &.via-slds {\n        .#{$namespace}-discount-item {\n            .#{$namespace}-discount-container {\n                .#{$namespace}-discount-price div {\n                    padding-right: 0;\n                    padding-left: $cpq-padding-large-horizontal;\n                }\n            }\n        }\n    }\n}",
