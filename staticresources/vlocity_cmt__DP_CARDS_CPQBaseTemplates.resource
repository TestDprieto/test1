                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-cart-promotions {\n            .#{$namespace}-promotion-items-tab-content {\n                padding-top: 0px; /* slds override */\n            }\n            .#{$namespace}-promotion-item-border {\n                padding: 10px 0px;\n                margin: 0px 16px;\n                border-bottom: 1px solid #d8dde6;\n            }\n            .#{$namespace}-base-promotion {\n                display: flex;\n                flex-flow: row nowrap;\n                justify-content: flex-end;\n\n                &.#{$namespace}-promotion-header {\n                    font-size: 11px;\n                    @extend .#{$namespace}-promotion-item-border;\n                }\n                &.#{$namespace}-promotion-item {\n                    @extend .#{$namespace}-promotion-item-border;\n                }\n                .#{$namespace}-promotion-name {\n                    flex: 5;\n                    padding: 0 6px 0 0;\n\n                    .#{$namespace}-promotion-item-bookmark-change, .#{$namespace}-promotion-item-bookmark-existing, .#{$namespace}-promotion-item-bookmark-disconnect {\n                        color: $product-item-bookmark-red;\n                    }\n                    .#{$namespace}-promotion-item-bookmark-add, .#{$namespace}-promotion-item-bookmark-default {\n                        color: $product-item-bookmark-blue;\n                    }  \n                }\n            }\n        }\n    }\n}",
