                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n\n/*** STYLES ***/\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-product-list {\n            display:flex;\n            flex: 1;\n            \n            .#{$namespace}-next-product {\n                 padding: $cpq-padding-large-vertical;\n                 \n                 .slds-spinner_container {\n                    position: relative;\n                    padding-top: 10px;\n                 }\n            }\n            //stretch the flex box irrespective of its contents\n            .#{$namespace}-products-list-vertical {\n                &.#{$namespace}-products-grid {\n                    flex-direction:column;\n             \n                    &.#{$namespace}-products-list-col-stretch{\n                        flex:1 1 100%;\n                    }\n                }\n                .#{$namespace}-no-products-msg {\n                    margin:auto;\n                }\n            }\n        }\n    }\n}\n",
