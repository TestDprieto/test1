                        "%vlocity_namespace%__HTML__c": "<div class=\"cpq-discount-item-details slds-scrollable_y\">\n    <div class=\"slds-grid slds-gutters\">\n        <div class=\"slds-col\">\n            <div class=\"slds-tile__title slds-text-heading_medium\">{{records.Name}}</div>\n            <div>{{records[$root.nsPrefix+ 'Description__c']}}</div>\n        </div>\n        <div class=\"slds-col\">\n            <div class=\"slds-tile__title slds-text-heading_medium slds-text-align_right cpq-discount-code-label\">{{::importedScope.customLabels.CPQDiscountCode}}</div>\n            <div class=\"slds-text-align_right\">{{records[$root.nsPrefix+ 'Code__c']}}</div>\n        </div>\n    </div>\n\n    <div class=\"cpq-discount-item-table slds-m-top_x-large\">\n        <div class=\"cpq-discount-item-table-row cpq-discount-item-table-header\">\n            <div class=\"slds-text-heading_label cpq-discount-item-table-row-item cpq-discount-item-table-flex-product\">{{::importedScope.customLabels.CPQEffectiveUntil}}</div>\n            <div class=\"slds-text-heading_label cpq-discount-item-table-row-item\">\n                {{::importedScope.customLabels.CPQDiscountType}}\n            </div>\n            <div class=\"slds-text-heading_label cpq-discount-item-table-row-item\">\n                {{::importedScope.customLabels.OneTime}}\n            </div>\n            <div class=\"slds-text-heading_label cpq-discount-item-table-row-item\">\n                {{::importedScope.customLabels.CPQMonthly}}\n            </div>\n            <div class=\"slds-text-heading_label cpq-discount-item-table-row-item\">\n                {{::importedScope.customLabels.CPQTimePlan}}\n            </div>\n        </div>\n        <div class=\"cpq-discount-item-table-row\">\n            <div class=\"slds-m-top_x-small cpq-discount-item-table-row-item cpq-discount-item-table-flex-product slds-tile__title slds-truncate cpq-cart-item-title\">\n                <span ng-bind-html=\"importedScope.convertToLocalDate(records[$root.nsPrefix+ 'effectiveenddate__c'])\"></span>\n            </div>\n            <div class=\"slds-m-top_x-small cpq-discount-item-table-row-item cpq-discount-item-table-flex-recur\">\n                {{records[$root.nsPrefix+ 'DiscountType__c']}}\n            </div>\n            <div class=\"slds-m-top_x-small cpq-discount-item-table-row-item cpq-discount-item-table-flex-onetime\">\n                <div ng-repeat=\"discountType in records['Discount']\"> \n                <span ng-show=\"discountType.chargeType === 'One-time'\">{{discountType.value}} {{discountType.selectedAdjustmentMethod}}</span>\n                </div>\n            </div>\n            <div class=\"slds-m-top_x-small cpq-discount-item-table-row-item cpq-discount-item-table-flex-onetime\">\n                <div ng-repeat=\"discountType in records['Discount']\"> \n                    <span ng-show=\"discountType.chargeType === 'Recurring'\">{{discountType.value}} {{discountType.selectedAdjustmentMethod}}</span>\n                </div>\n            </div>\n            <div class=\"slds-m-top_x-small  cpq-discount-item-table-row-item cpq-discount-item-table-flex-onetime\">\n                {{records[$root.nsPrefix+ 'Duration__c']}} {{records[$root.nsPrefix+ 'DurationUnitOfMeasure__c']}}\n            </div>\n        </div>\n    </div>\n\n    <!-- Start Scope -->\n    <div class=\"cpq-new-discount-container slds-m-bottom_small\">\n        <div class=\"slds-size_1-of-1 slds-m-top_x-large slds-m-bottom_small\">\n            <div class=\"cpq-custom-discount-criteria\">{{::importedScope.customLabels.CPQDiscountScope}}</div>\n        </div>\n        <table class=\"slds-table slds-table_bordered cpq-discount--table_border\">\n            <tr ng-if=\"records[$root.nsPrefix +'AppliesToAllItems__c']\">\n                <td class=\"slds-cell-shrink cpq-discount-cell-shrink\">\n                    <div class=\"slds-text-heading_label\">{{::importedScope.customLabels.CPQAppliesToAllItems}}</div