        {
            "ExternalId": "addContractStatusPicklistValues",
            "UniqueId": "addContractStatusPicklistValues",
            "Name": "Add Contract Status Picklist Values (New Installation)",
            "Priority": "None",
            "Tags": "CLM",
            "VersionIntroduced": "",
            "IsActive": true,
            "Type": "Manual",
            "StepFrequency": "First Install",
            "Instruction": "<p>1. Using the App Launcher, open Contracts.</p><p>2. Click the Setup icon, and choose Edit Object.</p><p>3. On the left-hand side, choose Fields &amp; Relationships.</p><p>4. Click Status.</p><p>5. Scroll down to Contract Status Picklist Values, and add the following values:</p><p class=\"ql-indent-1\">* Values - In Approval Process | API Name - In Approval Process | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Activated | API Name - Activated | Status Category - Activated</p><p class=\"ql-indent-1\">* Values - Draft | API Name - Draft | Status Category - Draft</p><p class=\"ql-indent-1\">* Values - Awaiting Approval | API Name - Awaiting Approval | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Awaiting Signature | API Name - Awaiting Signature | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Approved | API Name - Approved | Status Category - Activated</p><p class=\"ql-indent-1\">* Values - Cancelled | API Name - Cancelled | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Expired | API Name - Expired | Status Category - Activated</p><p class=\"ql-indent-1\">* Values - Negotiating | API Name - Negotiating | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Rejected Internally | API Name - Rejected Internally | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Signature Declined | API Name - Signature Declined | Status Category - In Approval Process</p><p class=\"ql-indent-1\">* Values - Signed | API Name - Signed | Status Category - Activated</p><p class=\"ql-indent-1\">* Values - Terminated | API Name - Terminated | Status Category - Activated</p><p>6. Click Save.</p>",
            "CustomInputLabel": "",
            "CustomInputValue": "",
            "Options": {
                "SObjectQuery": {
                    "QueryWhere": "WHERE MasterLabel IN ('In Approval Process', 'Activated', 'Draft', 'Awaiting Approval', 'Awaiting Signature', 'Approved', 'Cancelled', 'Expired', 'Negotiating', 'Rejected Internally', 'Signature Declined', 'Signed', 'Terminated') AND ApiName IN ('In Approval Process', 'Activated', 'Draft', 'Awaiting Approval', 'Awaiting Signature', 'Approved', 'Cancelled', 'Expired', 'Negotiating', 'Rejected Internally', 'Signature Declined', 'Signed', 'Terminated') AND StatusCode IN ('Activated', 'Draft', 'InApproval')",
                    "Compare": "==13"
                },
                "SObject": {
                    "SObjectType": "ContractStatus"
                }
            },
            "ArgumentList": "",
            "StepCommand": "",
            "CheckResultUrl": "/lightning/setup/ObjectManager/Contract/FieldsAndRelationships/Status/view",
            "PrecursorStepUniqueId": "",
            "ParentStepUniqueId": "",
            "PreCheckMethod": "",
            "PostCheckMethod": "VlocityDiagnosticWizardBaseMethods.checkSObjectsExist",
            "ExecutionMethod": ""
        },
